    "schemas": {
      "Cisco-IOS-XE-isis-intf-oper-isis-intf-oper-data": {
        "type": "object",
        "properties": {
          "if-name": {
            "type": "string",
            "description": "Interface name on which adjacency is formed"
          },
          "is-enabled": {
            "type": "boolean",
            "description": "Is the interface enabled for ISIS"
          },
          "is-passive": {
            "type": "boolean",
            "description": "Is the interface passive"
          },
          "hello-padding": {
            "type": "string",
            "description": "Defines hello-padding type on this interface"
          },
          "circuit-type": {
            "type": "string",
            "description": "Defines ISIS interface type"
          },
          "nbr-ipv4-addr": {
            "type": "string",
            "description": "IPv4 Address of the neighbor"
          },
          "nbr-ipv6-addr": {
            "type": "string",
            "format": "ipv6",
            "description": "IPv6 Address of the neighbor"
          },
          "adj-state": {
            "type": "string",
            "description": "Current state of the adjacency"
          },
          "nbr-snpa": {
            "type": "string",
            "pattern": "^([0-9A-Fa-f]{2}[:-]){5}[0-9A-Fa-f]{2}$",
            "description": "ISIS Neighbor sub-network point of attachment"
          },
          "local-ext-ckt-id": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "Local extended circuit ID"
          },
          "nbr-ext-ckt-id": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "Neighbor extended circuit ID"
          },
          "pri": {
            "type": "integer",
            "minimum": 0,
            "maximum": 255,
            "description": "Priority of the neighboring IS"
          },
          "dis-system-id": {
            "type": "string",
            "description": "Designated intermediate system id"
          },
          "nbr-ckt-type": {
            "type": "string",
            "description": "Received ISIS circuit type"
          },
          "adj-type": {
            "type": "string",
            "description": "Formed ISIS adjacency type"
          },
          "up-time": {
            "type": "string",
            "description": "Time at which adjacency transitioned into UP state"
          },
          "restart-support": {
            "type": "boolean",
            "description": "When set to true, ISIS graceful restart is supported"
          },
          "restart-status": {
            "type": "boolean",
            "description": "When set to true, neighbor is being helped"
          },
          "multi-topo": {
            "type": "boolean",
            "description": "When set to true, ISIS multi-topology is supported"
          },
          "nlpid": {
            "type": "array",
            "items": {
              "type": "integer",
              "minimum": 0,
              "maximum": 255,
              "description": "Supported protocol, as per TLV 129"
            }
          },
          "topo": {
            "type": "array",
            "items": {
              "type": "integer",
              "minimum": 0,
              "maximum": 4294967295,
              "description": "List of ISIS multi-topology IDs supported\n             MT ID 0: Equivalent to the standard topology\n             MT ID 1: IPv4 in-band management purposes\n             MT ID 2: IPv6 routing topology\n             MT ID 3: IPv4 multicast routing topology\n             MT ID 4: IPv6 multicast routing topology\n             MT ID 5: IPv6 in-band management purposes\n             MT ID 6-3995: Reserved for IETF consensus"
            }
          },
          "area-addr": {
            "type": "array",
            "items": {
              "type": "string",
              "description": "List of ISIS area addresses"
            }
          },
          "system-id": {
            "type": "string",
            "description": "System ID of the neighbor"
          },
          "level": {
            "type": "string",
            "description": "Level of the adjacency formed"
          },
          "tag": {
            "type": "string",
            "description": "IS-IS router tag"
          }
        }
      },
