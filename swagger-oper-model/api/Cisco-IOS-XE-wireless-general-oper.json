          },
          "intf-type": {
            "type": "string",
            "description": "Controller management interface type"
          },
          "intf-id": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "Controller management interface id"
          },
          "mgmt-ip": {
            "type": "string",
            "pattern": "^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}$",
            "description": "Controller management IPv4 address"
          },
          "net-mask": {
            "type": "string",
            "pattern": "^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}$",
            "description": "Controller management interface netmask"
          },
          "mgmt-mac": {
            "type": "string",
            "pattern": "^([0-9A-Fa-f]{2}[:-]){5}[0-9A-Fa-f]{2}$",
            "description": "Controller management interface MAC"
          },
          "count": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "Number of occurrences of this event"
          },
          "event": {
            "type": "string",
            "description": "Event name"
          },
          "frames": {
            "type": "string",
            "description": "List of frames seen with this event"
          },
          "bssid": {
            "type": "string",
            "pattern": "^([0-9A-Fa-f]{2}[:-]){5}[0-9A-Fa-f]{2}$",
            "description": "BSSID detected as anomalous"
          },
          "wtp-mac": {
            "type": "string",
