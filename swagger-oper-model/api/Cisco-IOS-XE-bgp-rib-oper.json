    "schemas": {
      "Cisco-IOS-XE-bgp-rib-oper-bgp-rib-oper-data": {
        "type": "object",
        "properties": {
          "prefix": {
            "type": "string",
            "description": "The BGP prefix associated with the route entry."
          },
          "ni-type": {
            "type": "string",
            "description": "The type of network instance (e.g., VRF, global) associated with the route entry."
          },
          "ni-name": {
            "type": "string",
            "description": "The name of the network instance associated with the route entry."
          },
          "afi-safi": {
            "type": "string",
            "description": "The address family identifier (AFI) and subsequent address family identifier (SAFI) for the route entry."
          },
          "src-orig": {
            "type": "string",
            "description": "The origin of the BGP path, indicating how the path was learned.It is either the source protocol or the neighbor address."
          },
          "nbr-addr": {
            "type": "string",
            "description": "The neighbor address from which the BGP path was learned."
          },
          "if-name": {
            "type": "string",
            "description": "Contains interface name if the next-hop is a link-local address, empty otherwise."
          },
          "src-rd": {
            "type": "string",
            "description": "The source route distinguisher associated with this path if this path was imported from another vrf, empty otherwise."
          },
          "src-ni-name": {
            "type": "string",
            "description": "The name of the network instance associated with this path if this path was imported from another vrf, empty otherwise."
          },
          "path-id": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "A unique identifier for the BGP path, used to differentiate between multiple paths for the same prefix."
          },
          "type": {
            "type": "integer",
            "minimum": 0,
            "maximum": 255,
            "description": "BGP path unknown attribute type."
          },
          "len": {
            "type": "integer",
            "minimum": 0,
            "maximum": 65535,
            "description": "BGP path unknown attribute length."
          },
          "val": {
            "type": "string",
            "description": "BGP path unknown attribute value."
          },
          "fg": {
            "type": "string",
            "description": "BGP path unknown attribute flags."
          },
          "path-st": {
            "type": "string",
            "description": "The current status of the BGP path."
          },
          "rpki-st": {
            "type": "string",
            "description": "The RPKI (Resource Public Key Infrastructure) status of the BGP path."
          },
          "mpls-in": {
            "type": "string",
            "description": "The MPLS (Multiprotocol Label Switching) label for incoming traffic on the path."
          },
          "mpls-out": {
            "type": "string",
            "description": "The MPLS (Multiprotocol Label Switching) label for outgoing traffic on the path."
          },
          "sr-prof-name": {
            "type": "string",
            "description": "The Segment Routing (SR) profile name associated with the path."
          },
          "sr-bg-sid": {
            "type": "string",
            "description": "The Segment Routing (SR) binding SID (Segment Identifier) for the path."
          },
          "sr-lbl-indx": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "The Segment Routing (SR) label index for the path."
          },
          "attr-idx": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "Index to the BGP attribute associated with the path."
          },
          "comm-idx": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "Index to the BGP community associated with the path."
          },
          "extcomm-idx": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "Index to the BGP extended community associated with the path."
          },
          "lrgcomm-idx": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "Index to the BGP large community associated with the path."
          },
          "version": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "BGP Prefix update version."
          },
          "avail-paths": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "The number of available BGP paths for the prefix, indicating the different routes that can be used to reach the destination."
          },
          "attr-id": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "Attribute Index."
          },
          "as-value": {
            "type": "string",
            "description": "The Autonomous System (AS) number value associated with the BGP path."
          },
          "as-type": {
            "type": "string",
            "description": "BGP AS Path Type."
          },
          "nh": {
            "type": "string",
            "description": "The next-hop IP address for the BGP route, indicating the immediate next router to which packets should be forwarded."
          },
          "metric": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "Metric associated with the path, used for determining the best path among multiple available paths."
          },
          "lp": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "Local preference for this path, used to prefer one path over another within the same AS."
          },
          "weight": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "Path weight, a Cisco-specific attribute used to select the best path when multiple paths are available."
          },
          "origin": {
            "type": "string",
            "description": "Path origin, indicating how BGP learned about the route (IGP, EGP, or INCOMPLETE)."
          },
          "aa": {
            "type": "boolean",
            "description": "Attribute indicating whether or not the prefix is an atomic aggregate, meaning it has been summarized."
          },
