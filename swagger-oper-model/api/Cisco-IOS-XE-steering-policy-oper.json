          },
          "weight": {
            "type": "integer",
            "minimum": 0,
            "maximum": 255,
            "description": "A load-balancing metric used for traffic distribution among RLOCs"
          },
          "priority": {
            "type": "integer",
            "minimum": 0,
            "maximum": 255,
            "description": "Sets the priority level for using this RLOC. A lower number has higher priority"
          },
          "best-rloc": {
            "type": "boolean",
            "description": "Best RLOC based on weight and priority"
          },
          "name": {
            "type": "string",
            "description": "The name assigned to the policy"
          },
          "ref-count": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295,
            "description": "Number of policy instances in which this service is used"
          },
          "rloc-qry-status": {
            "type": "string",
            "description": "Represents the query status of the RLOC information from LISP map-server"
          },
          "extranet-iid": {
            "type": "integer",
            "minimum": -2147483648,
            "maximum": 2147483647,
            "description": "Identifies the extranet associated with the service"
          },
          "num-of-rlocs": {
            "type": "integer",
            "minimum": 0,
            "maximum": 255,
            "description": "The count of RLOCs associated with the service"
          },
          "status": {
            "type": "string",
            "description": "Policy provision status"
