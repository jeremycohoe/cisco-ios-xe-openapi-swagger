{
  "openapi": "3.0.0",
  "info": {
    "title": "Other - common-mpls-static",
    "version": "17.18.1",
    "description": "This module contains a collection of YANG definitions for\n     configuring static MPLS LSPs.\n\n**Authentication**: HTTP Basic Authentication\n\n**Protocol**: RESTCONF (RFC 8040)\n\n**Base Path**: https://{device}/restconf/data\n\n**YANG Model:** [common-mpls-static.yang](https://github.com/YangModels/yang/blob/main/vendor/cisco/xe/17181/common-mpls-static.yang)\n\n**ðŸ“Š YANG Tree:** [View common-mpls-static structure](https://jeremycohoe.github.io/cisco-ios-xe-openapi-swagger/yang-trees/common-mpls-static.html)"
  },
  "servers": [
    {
      "url": "https://{device}/restconf",
      "variables": {
        "device": {
          "default": "sandbox-iosxe-latest-1.cisco.com",
          "description": "Device IP or hostname"
        }
      }
    }
  ],
  "paths": {
    "/data/common-mpls-static:mpls-static": {
      "get": {
        "summary": "Get mpls-static",
        "description": "Retrieve static MPLS LSP configuration",
        "operationId": "getMplsStatic",
        "tags": ["common-mpls-static"],
        "parameters": [
          {
            "name": "depth",
            "in": "query",
            "description": "Query depth - unbounded or specific integer",
            "schema": {
              "type": "string",
              "default": "unbounded"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "$ref": "#/components/schemas/MplsStatic"
                },
                "example": {
                  "common-mpls-static:mpls-static": {
                    "mpls-static-cfg": {
                      "interfaces": {
                        "interface": [
                          {
                            "name": "GigabitEthernet1",
                            "enabled": 1
                          }
                        ]
                      },
                      "ipv4-ingress-lsps": {
                        "ipv4-ingress-lsp": [
                          {
                            "vrf-name": "default",
                            "prefix": "10.1.1.0/24",
                            "name": "lsp1",
                            "in-label": 100
                          }
                        ]
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized - authentication required"
          },
          "404": {
            "description": "Resource not found"
          }
        }
      },
      "put": {
        "summary": "Replace mpls-static",
        "description": "Replace entire static MPLS configuration",
        "operationId": "replaceMplsStatic",
        "tags": ["common-mpls-static"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "$ref": "#/components/schemas/MplsStatic"
              },
              "example": {
                "common-mpls-static:mpls-static": {
                  "mpls-static-cfg": {
                    "interfaces": {
                      "interface": [
                        {
                          "name": "GigabitEthernet1",
                          "enabled": 1
                        }
                      ]
                    },
                    "ipv4-ingress-lsps": {
                      "ipv4-ingress-lsp": [
                        {
                          "vrf-name": "default",
                          "prefix": "10.1.1.0/24",
                          "name": "lsp1",
                          "in-label": 100
                        }
                      ]
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "Successfully replaced"
          },
          "400": {
            "description": "Bad request - validation error"
          },
          "401": {
            "description": "Unauthorized - authentication required"
          }
        }
      },
      "patch": {
        "summary": "Update mpls-static",
        "description": "Partially update static MPLS configuration",
        "operationId": "patchMplsStatic",
        "tags": ["common-mpls-static"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "$ref": "#/components/schemas/MplsStatic"
              },
              "example": {
                "common-mpls-static:mpls-static": {
                  "mpls-static-cfg": {
                    "interfaces": {
                      "interface": [
                        {
                          "name": "GigabitEthernet2",
                          "enabled": 1
                        }
                      ]
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "Successfully updated"
          },
          "400": {
            "description": "Bad request - validation error"
          },
          "401": {
            "description": "Unauthorized - authentication required"
          }
        }
      },
      "delete": {
        "summary": "Delete mpls-static",
        "description": "Remove static MPLS configuration",
        "operationId": "deleteMplsStatic",
        "tags": ["common-mpls-static"],
        "responses": {
          "204": {
            "description": "Successfully deleted"
          },
          "401": {
            "description": "Unauthorized - authentication required"
          },
          "404": {
            "description": "Resource not found"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "MplsStatic": {
        "type": "object",
        "properties": {
          "common-mpls-static:mpls-static": {
            "type": "object",
            "description": "Static MPLS configuration container",
            "properties": {
              "mpls-static-cfg": {
                "type": "object",
                "properties": {
                  "interfaces": {
                    "type": "object",
                    "description": "MPLS-enabled interfaces",
                    "properties": {
                      "interface": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "name": {
                              "type": "string",
                              "description": "Interface reference"
                            },
                            "enabled": {
                              "type": "integer",
                              "format": "uint32",
                              "description": "Enable MPLS on interface (1=enabled)"
                            }
                          }
                        }
                      }
                    }
                  },
                  "ipv4-ingress-lsps": {
                    "type": "object",
                    "description": "IPv4 ingress LSP configuration",
                    "properties": {
                      "ipv4-ingress-lsp": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "vrf-name": {
                              "type": "string",
                              "description": "VRF name"
                            },
                            "prefix": {
                              "type": "string",
                              "format": "ipv4-prefix",
                              "description": "IPv4 prefix for LSP"
                            },
                            "name": {
                              "type": "string",
                              "description": "LSP name"
                            },
                            "in-label": {
                              "type": "integer",
                              "description": "Incoming MPLS label"
                            }
                          }
                        }
                      }
                    }
                  },
                  "ipv6-ingress-lsps": {
                    "type": "object",
                    "description": "IPv6 ingress LSP configuration"
                  }
                }
              }
            }
          }
        }
      }
    },
    "securitySchemes": {
      "BasicAuth": {
        "type": "http",
        "scheme": "basic"
      }
    }
  },
  "security": [
    {
      "BasicAuth": []
    }
  ],
  "tags": [
    {
      "name": "common-mpls-static",
      "description": "Operations for static MPLS LSP configuration"
    }
  ]
}
