      "get": {
        "summary": "Get ethernet data",
        "description": "Retrieve ethernet configuration/operational data",
        "tags": [
          "cisco-ethernet"
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "$ref": "#/components/schemas/cisco-ethernet_ethernet"
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Update ethernet",
        "description": "Update or create ethernet configuration",
        "tags": [
          "cisco-ethernet"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "$ref": "#/components/schemas/cisco-ethernet_ethernet"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          },
          "204": {
            "description": "Updated"
          }
        }
      }
    },
