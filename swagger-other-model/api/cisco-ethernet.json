    }
  },
  "servers": [
    {
      "url": "https://{device}:{port}/restconf",
      "variables": {
        "device": {
          "default": "ios-xe-device",
          "description": "IOS-XE Device IP or Hostname"
        },
        "port": {
          "default": "443",
          "description": "RESTCONF Port"
        }
      }
    }
  ],
  "paths": {
