{
  "openapi": "3.0.0",
  "info": {
    "title": "Cisco IOS-XE Native Config - IP Configuration",
    "description": "IP networking configuration for Cisco-IOS-XE-native model. This container includes routing, access-lists, services, DHCP, DNS, NAT, and all IPv4-related configuration.\n\n**What's included:** Complete IP configuration hierarchy under /native/ip\n\n**Common use cases:** Static routing, ACLs, IP services, DHCP server, DNS configuration, source routing, routing protocols access\n\n**Related:** See 'Router' for dynamic routing protocols (OSPF, BGP), 'IPv6' for IPv6 configuration",
    "version": "17.18.1",
    "contact": {
      "name": "Cisco IOS-XE RESTCONF API",
      "url": "https://developer.cisco.com/docs/ios-xe/"
    }
  },
  "servers": [
    {
      "url": "https://{device}:{port}",
      "description": "IOS-XE Device RESTCONF API",
      "variables": {
        "device": {
          "default": "devnetsandboxiosxec9k.cisco.com",
          "description": "Device IP or hostname"
        },
        "port": {
          "default": "443",
          "description": "HTTPS port"
        }
      }
    }
  ],
  "paths": {
    "/data/Cisco-IOS-XE-native:native/ip": {
      "get": {
        "summary": "Get all IP configuration",
        "description": "Retrieve entire IP configuration container",
        "operationId": "get-ip-all",
        "tags": [
          "ip-root"
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "Cisco-IOS-XE-native:ip": {
                      "type": "object",
                      "description": "Complete IP configuration"
                    }
                  }
                },
                "example": {
                  "Cisco-IOS-XE-native:ip": {
                    "domain": {
                      "name": "example.com"
                    },
                    "source-route": false
                  }
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Replace IP configuration",
        "description": "Replace entire IP configuration",
        "operationId": "put-ip-all",
        "tags": [
          "ip-root"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "Cisco-IOS-XE-native:ip": {
                    "type": "object"
                  }
                }
              },
              "example": {
                "Cisco-IOS-XE-native:ip": {
                  "domain": {
                    "name": "example.com"
                  },
                  "source-route": false
                }
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "Successfully configured"
          }
        }
      },
      "patch": {
        "summary": "Update IP configuration",
        "description": "Merge update to IP configuration",
        "operationId": "patch-ip-all",
        "tags": [
          "ip-root"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "Cisco-IOS-XE-native:ip": {
                    "type": "object"
                  }
                }
              },
              "example": {
                "Cisco-IOS-XE-native:ip": {
                  "domain": {
                    "lookup": false
                  }
                }
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "Successfully updated"
          }
        }
      },
      "delete": {
        "summary": "Delete IP configuration",
        "description": "Remove all IP configuration (WARNING: disrupts routing and services)",
        "operationId": "delete-ip-all",
        "tags": [
          "ip-root"
        ],
        "responses": {
          "204": {
            "description": "Successfully deleted"
          }
        }
      }
    },
