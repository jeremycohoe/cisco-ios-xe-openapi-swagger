{
  "openapi": "3.0.0",
  "info": {
    "title": "Cisco IOS-XE Native Config - QoS & Policy",
    "description": "Quality of Service configuration from Cisco-IOS-XE-native model.\n\n**Includes:** qos, parameter-map\n\n**Note:** The large 'policy' container (109K+ lines) is excluded from this spec for performance reasons.\n\n**Key Features:**\n- QoS global settings\n- Parameter maps\n- Traffic policies\n\n**Base Path:** `/data/Cisco-IOS-XE-native:native/`\n\n**Related:** See 'IP Configuration' for IP-based QoS features",
    "version": "17.18.1",
    "contact": {
      "name": "Cisco IOS-XE RESTCONF API",
      "url": "https://developer.cisco.com/docs/ios-xe/"
    }
  },
  "servers": [
    {
      "url": "https://{device}:{port}",
      "description": "IOS-XE Device RESTCONF API",
      "variables": {
        "device": {
          "default": "router.example.com",
          "description": "Device IP or hostname"
        },
        "port": {
          "default": "443",
          "description": "HTTPS port"
        }
      }
    }
  ],
  "paths": {
    "/data/Cisco-IOS-XE-native:native/qos": {
      "get": {"summary": "Get QoS configuration", "operationId": "get-qos", "tags": ["qos"], "responses": {"200": {"description": "Successful response"}}},
      "put": {"summary": "Configure QoS", "operationId": "put-qos", "tags": ["qos"], "requestBody": {"required": true, "content": {"application/yang-data+json": {"schema": {"type": "object"}}}}, "responses": {"204": {"description": "Successfully configured"}}},
      "patch": {"summary": "Update QoS", "operationId": "patch-qos", "tags": ["qos"], "requestBody": {"required": true, "content": {"application/yang-data+json": {"schema": {"type": "object"}}}}, "responses": {"204": {"description": "Successfully updated"}}},
      "delete": {"summary": "Delete QoS", "operationId": "delete-qos", "tags": ["qos"], "responses": {"204": {"description": "Successfully deleted"}}}
    },
    "/data/Cisco-IOS-XE-native:native/parameter-map": {
      "get": {"summary": "Get parameter maps", "operationId": "get-parameter-map", "tags": ["qos"], "responses": {"200": {"description": "Successful response"}}},
      "put": {"summary": "Configure parameter map", "operationId": "put-parameter-map", "tags": ["qos"], "requestBody": {"required": true, "content": {"application/yang-data+json": {"schema": {"type": "object"}}}}, "responses": {"204": {"description": "Successfully configured"}}},
      "patch": {"summary": "Update parameter map", "operationId": "patch-parameter-map", "tags": ["qos"], "requestBody": {"required": true, "content": {"application/yang-data+json": {"schema": {"type": "object"}}}}, "responses": {"204": {"description": "Successfully updated"}}},
      "delete": {"summary": "Delete parameter map", "operationId": "delete-parameter-map", "tags": ["qos"], "responses": {"204": {"description": "Successfully deleted"}}}
    }
  },
  "components": {
    "securitySchemes": {
      "basicAuth": {
        "type": "http",
        "scheme": "basic"
      }
    }
  },
  "security": [{"basicAuth": []}],
  "tags": [
    {"name": "qos", "description": "Quality of Service"}
  ]
}
