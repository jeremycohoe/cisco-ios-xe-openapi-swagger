          "200": {
            "description": "Successful response",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                },
                "example": {
                  "Cisco-IOS-XE-native:bridge-domain": {
                    "brd-id": "100",
                    "mac": {
                      "learning": true
                    }
                  }
                }
              }
            }
