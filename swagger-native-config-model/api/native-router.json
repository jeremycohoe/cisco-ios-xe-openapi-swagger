{
  "openapi": "3.0.0",
  "info": {
    "title": "Cisco IOS-XE Native Config - Routing Protocols",
    "description": "Dynamic routing protocol configuration for Cisco-IOS-XE-native model. This container includes OSPF, BGP, EIGRP, RIP, ISIS, and other routing protocol configurations.\n\n**What's included:** Complete router configuration hierarchy under /native/router\n\n**Supported protocols:** OSPF, BGP, EIGRP, RIP, ISIS, Mobile, ODR, LISP\n\n**Related:** See 'IP' for static routing and IP services",
    "version": "17.18.1",
    "contact": {
      "name": "Cisco IOS-XE RESTCONF API",
      "url": "https://developer.cisco.com/docs/ios-xe/"
    }
  },
  "servers": [
    {
      "url": "https://{device}:{port}",
      "description": "IOS-XE Device RESTCONF API",
      "variables": {
        "device": {
          "default": "router.example.com",
          "description": "Device IP or hostname"
        },
        "port": {
          "default": "443",
          "description": "HTTPS port"
        }
      }
    }
  ],
  "paths": {
    "/data/Cisco-IOS-XE-native:native/router": {
      "get": {
        "summary": "Get all routing protocols",
        "description": "Retrieve all routing protocol configurations",
        "operationId": "get-router-all",
        "tags": [
          "router-root"
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "Cisco-IOS-XE-native:router": {
                      "type": "object",
                      "description": "All routing protocol configuration"
                    }
                  }
                },
                "example": {
                  "Cisco-IOS-XE-native:router": {
                    "Cisco-IOS-XE-ospf:ospf": [
                      {
                        "id": 1,
                        "network": [
                          {
                            "ip": "10.0.0.0",
                            "wildcard": "0.255.255.255",
                            "area": 0
                          }
                        ]
                      }
                    ]
                  }
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Replace routing protocols",
        "description": "Replace all routing protocol configurations",
        "operationId": "put-router-all",
        "tags": [
          "router-root"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "Cisco-IOS-XE-native:router": {
                    "type": "object"
                  }
                }
              },
              "example": {
                "Cisco-IOS-XE-native:router": {
                  "Cisco-IOS-XE-ospf:ospf": [
                    {
                      "id": 1,
                      "network": [
                        {
                          "ip": "10.0.0.0",
                          "wildcard": "0.255.255.255",
                          "area": 0
                        }
                      ]
                    }
                  ]
                }
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "Successfully configured"
          }
        }
      },
      "patch": {
        "summary": "Update routing protocols",
        "description": "Merge update to routing protocol configurations",
        "operationId": "patch-router-all",
        "tags": [
          "router-root"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "Cisco-IOS-XE-native:router": {
                    "type": "object"
                  }
                }
              },
              "example": {
                "Cisco-IOS-XE-native:router": {
                  "Cisco-IOS-XE-bgp:bgp": [
                    {
                      "id": 65001,
                      "bgp": {
                        "router-id": "10.1.1.1"
                      }
                    }
                  ]
                }
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "Successfully updated"
          }
        }
      },
      "delete": {
        "summary": "Delete routing protocols",
        "description": "Remove all routing protocol configurations (WARNING: disrupts dynamic routing)",
        "operationId": "delete-router-all",
        "tags": [
          "router-root"
        ],
        "responses": {
          "204": {
            "description": "Successfully deleted"
          }
        }
      }
    },
