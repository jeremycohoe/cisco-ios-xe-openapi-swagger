{
  "openapi": "3.0.0",
  "info": {
    "title": "Cisco IOS-XE Native Config - Line Configuration (Console, VTY, AUX)",
    "description": "Line access configuration for Cisco-IOS-XE-native model. This container includes console, VTY (virtual terminal), and AUX line configurations for device access.\n\n**What's included:** Complete line configuration hierarchy under /native/line\n\n**Line types:** Console (CON), VTY (Telnet/SSH), AUX\n\n**Common use cases:** SSH/Telnet access, timeout settings, exec configuration, privilege levels, access-class\n\n**Related:** See 'AAA' for authentication methods, 'IP' for SSH configuration",
    "version": "17.18.1",
    "contact": {
      "name": "Cisco IOS-XE RESTCONF API",
      "url": "https://developer.cisco.com/docs/ios-xe/"
    }
  },
  "servers": [
    {
      "url": "https://{device}:{port}",
      "description": "IOS-XE Device RESTCONF API",
      "variables": {
        "device": {
          "default": "router.example.com",
          "description": "Device IP or hostname"
        },
        "port": {
          "default": "443",
          "description": "HTTPS port"
        }
      }
    }
  ],
  "paths": {
    "/data/Cisco-IOS-XE-native:native/line": {
      "get": {
        "summary": "Get all line configuration",
        "description": "Retrieve all line configurations (console, VTY, AUX)",
        "operationId": "get-line-all",
        "tags": ["line-root"],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "Cisco-IOS-XE-native:line": {
                      "type": "object",
                      "description": "Complete line configuration"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Replace line configuration",
        "description": "Replace all line configurations",
        "operationId": "put-line-all",
        "tags": ["line-root"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              },
              "example": {
                "Cisco-IOS-XE-native:line": {
                  "console": {
                    "first": "0",
                    "exec-timeout": {
                      "minutes": 5,
                      "seconds": 0
                    },
                    "logging": {
                      "synchronous": [null]
                    }
                  },
                  "vty": [
                    {
                      "first": 0,
                      "last": 4,
                      "transport": {
                        "input": {
                          "input": "ssh"
                        }
                      }
                    }
                  ]
                }
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully configured" }
        }
      },
      "patch": {
        "summary": "Update line configuration",
        "description": "Merge update to line configurations",
        "operationId": "patch-line-all",
        "tags": ["line-root"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              },
              "example": {
                "Cisco-IOS-XE-native:line": {
                  "vty": [
                    {
                      "first": 0,
                      "last": 15,
                      "exec-timeout": {
                        "minutes": 10,
                        "seconds": 0
                      }
                    }
                  ]
                }
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully updated" }
        }
      },
      "delete": {
        "summary": "Delete line configuration",
        "description": "Remove all line configurations (WARNING: may lock out CLI access)",
        "operationId": "delete-line-all",
        "tags": ["line-root"],
        "responses": {
          "204": { "description": "Successfully deleted" }
        }
      }
    },
    "/data/Cisco-IOS-XE-native:native/line/console": {
      "get": {
        "summary": "Get console line configuration",
        "description": "Retrieve console line configuration",
        "operationId": "get-line-console",
        "tags": ["console"],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Configure console line",
        "description": "Replace console line configuration",
        "operationId": "put-line-console",
        "tags": ["console"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              },
              "example": {
                "Cisco-IOS-XE-native:console": {
                  "first": "0",
                  "exec-timeout": {
                    "minutes": 0,
                    "seconds": 0
                  },
                  "logging": {
                    "synchronous": [null]
                  },
                  "stopbits": "1"
                }
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully configured" }
        }
      },
      "patch": {
        "summary": "Update console line configuration",
        "description": "Merge update to console line configuration",
        "operationId": "patch-line-console",
        "tags": ["console"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              },
              "example": {
                "Cisco-IOS-XE-native:console": {
                  "first": "0",
                  "privilege": {
                    "level": {
                      "number": 15
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully updated" }
        }
      },
      "delete": {
        "summary": "Delete console line configuration",
        "description": "Remove console line configuration",
        "operationId": "delete-line-console",
        "tags": ["console"],
        "responses": {
          "204": { "description": "Successfully deleted" }
        }
      }
    },
    "/data/Cisco-IOS-XE-native:native/line/vty": {
      "get": {
        "summary": "Get VTY line configuration",
        "description": "Retrieve VTY (Telnet/SSH) line configuration",
        "operationId": "get-line-vty",
        "tags": ["vty"],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Configure VTY lines",
        "description": "Replace VTY line configuration",
        "operationId": "put-line-vty",
        "tags": ["vty"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              },
              "example": {
                "Cisco-IOS-XE-native:vty": [
                  {
                    "first": 0,
                    "last": 4,
                    "transport": {
                      "input": {
                        "input": "ssh"
                      }
                    },
                    "access-class": {
                      "access-class-in": {
                        "direction": "in",
                        "acl": {
                          "acl-name": "SSH-ACCESS"
                        }
                      }
                    }
                  }
                ]
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully configured" }
        }
      },
      "patch": {
        "summary": "Update VTY line configuration",
        "description": "Merge update to VTY line configuration",
        "operationId": "patch-line-vty",
        "tags": ["vty"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              },
              "example": {
                "Cisco-IOS-XE-native:vty": [
                  {
                    "first": 5,
                    "last": 15,
                    "exec-timeout": {
                      "minutes": 30,
                      "seconds": 0
                    }
                  }
                ]
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully updated" }
        }
      },
      "delete": {
        "summary": "Delete VTY line configuration",
        "description": "Remove VTY line configuration (WARNING: locks out SSH/Telnet)",
        "operationId": "delete-line-vty",
        "tags": ["vty"],
        "responses": {
          "204": { "description": "Successfully deleted" }
        }
      }
    },
    "/data/Cisco-IOS-XE-native:native/line/aux": {
      "get": {
        "summary": "Get AUX line configuration",
        "description": "Retrieve auxiliary port line configuration",
        "operationId": "get-line-aux",
        "tags": ["aux"],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Configure AUX line",
        "description": "Replace AUX line configuration",
        "operationId": "put-line-aux",
        "tags": ["aux"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              },
              "example": {
                "Cisco-IOS-XE-native:aux": {
                  "first": "0",
                  "exec-timeout": {
                    "minutes": 10,
                    "seconds": 0
                  },
                  "stopbits": "1"
                }
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully configured" }
        }
      },
      "patch": {
        "summary": "Update AUX line configuration",
        "description": "Merge update to AUX line configuration",
        "operationId": "patch-line-aux",
        "tags": ["aux"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              },
              "example": {
                "Cisco-IOS-XE-native:aux": {
                  "first": "0",
                  "speed": "115200"
                }
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully updated" }
        }
      },
      "delete": {
        "summary": "Delete AUX line configuration",
        "description": "Remove AUX line configuration",
        "operationId": "delete-line-aux",
        "tags": ["aux"],
        "responses": {
          "204": { "description": "Successfully deleted" }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "basicAuth": {
        "type": "http",
        "scheme": "basic"
      }
    }
  },
