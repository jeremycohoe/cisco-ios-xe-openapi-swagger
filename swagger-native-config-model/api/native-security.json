  "components": {
    "securitySchemes": {
      "basicAuth": {
        "type": "http",
        "scheme": "basic"
      }
    },
    "schemas": {
      "native-enable": {
        "type": "object",
        "properties": {
          "last-resort": {
            "type": "string",
            "enum": [
              "password",
              "succeed"
            ],
            "description": "Define enable action if no TACACS servers respond"
          },
          "level": {
            "type": "integer",
            "minimum": 0,
            "maximum": 255,
            "description": "Set exec level password"
          },
          "type": {
            "type": "string",
            "enum": [
              "0",
              "4",
              "5",
              "8",
              "9"
            ]
          },
          "secret": {
            "type": "object",
            "properties": {
              "level": {
                "type": "integer",
                "minimum": 0,
                "maximum": 255,
                "description": "Set exec level password"
              },
              "type": {
                "type": "string",
                "enum": [
                  "0",
                  "4",
                  "5",
                  "8",
                  "9"
                ]
              },
              "secret": {
                "type": "string"
              }
            }
          },
          "use-tacacs": {
            "type": "boolean",
            "description": "Use TACACS to check enable passwords"
          },
          "password": {
            "type": "object"
          }
        }
      },
      "native-aaa": {
        "type": "object"
      },
      "native-parameter-map": {
        "type": "object",
        "properties": {
          "type": {
            "type": "object"
          }
        }
      },
      "native-zone": {
        "type": "object"
      },
      "native-zone-pair": {
        "type": "object"
      },
      "native-crypto": {
        "type": "object"
      },
      "native-username": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "description": "User name with max string length 64"
            },
            "privilege": {
              "type": "integer",
              "minimum": 0,
              "maximum": 255,
              "description": "Set user privilege level"
            },
            "view": {
              "type": "string",
              "description": "Set view name with max string length 30"
            },
            "one-time": {
              "type": "boolean",
              "description": "Specify that the username/password is valid for only one time"
            },
            "mac": {
              "type": "boolean",
              "description": "Entry is for MAC Filtering"
            },
            "serial-number": {
              "type": "boolean",
              "description": "Entry is for serial number"
            },
            "description": {
              "type": "string",
              "description": "description string with max 128 characters"
            },
            "wlan-profile-name": {
              "type": "string",
              "description": "wlan-profile-name string with max 32 characters"
            },
            "no-password": {
              "type": "boolean",
              "description": "No password is required for the user to log in"
            },
            "encryption": {
              "type": "string",
              "enum": [
                "0",
                "5",
                "8",
                "9"
              ]
            },
            "password": {
              "type": "object",
              "properties": {
                "encryption": {
                  "type": "string",
                  "enum": [
                    "0",
                    "7",
                    "6"
                  ]
                },
                "password": {
                  "type": "string"
                }
              }
            },
            "secret": {
              "type": "object",
              "properties": {
                "encryption": {
                  "type": "string",
                  "enum": [
                    "0",
                    "5",
                    "8",
                    "9"
                  ]
                },
                "secret": {
                  "type": "string"
                }
              }
            }
          }
        }
      },
      "native-username-item": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "User name with max string length 64"
          },
          "privilege": {
