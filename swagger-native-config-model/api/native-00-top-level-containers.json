      "put": {"summary": "Configure SNMP", "operationId": "put-snmp-server", "tags": ["containers", "management"], "requestBody": {"required": true, "content": {"application/yang-data+json": {"schema": {"type": "object"}}}}, "responses": {"204": {"description": "Successfully configured"}}},
      "patch": {"summary": "Update SNMP", "operationId": "patch-snmp-server", "tags": ["containers", "management"], "requestBody": {"required": true, "content": {"application/yang-data+json": {"schema": {"type": "object"}}}}, "responses": {"204": {"description": "Successfully updated"}}},
      "delete": {"summary": "Delete SNMP", "operationId": "delete-snmp-server", "tags": ["containers", "management"], "responses": {"204": {"description": "Successfully deleted"}}}
