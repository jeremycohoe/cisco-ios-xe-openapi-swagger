      "patch": {"summary": "Update NAT", "operationId": "patch-nat", "tags": ["containers", "services"], "requestBody": {"required": true, "content": {"application/yang-data+json": {"schema": {"type": "object"}}}}, "responses": {"204": {"description": "Successfully updated"}}},
