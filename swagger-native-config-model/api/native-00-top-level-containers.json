{
  "openapi": "3.0.0",
  "info": {
    "title": "Cisco IOS-XE Native Config - Top-Level Containers (Small/Miscellaneous)",
    "description": "Smaller top-level container nodes in the Cisco-IOS-XE-native model. Large/complex containers (ip, router, crypto, aaa, line, vrf, interface) have dedicated category pages.\n\n**What's included:** Small/medium containers: vlan, logging, snmp-server, nat, spanning-tree, policy, segment-routing, license, platform, service, controller, and others.\n\n**For larger containers, see dedicated pages:** IP, Router, Crypto, AAA, Line, VRF\n\n**Note:** Interface container excluded (too large for Swagger UI)\n\n**Related:** See 'Top-Level Leafs' for simple values (hostname, version, etc.)",
    "version": "17.18.1",
    "contact": {
      "name": "Cisco IOS-XE RESTCONF API",
      "url": "https://developer.cisco.com/docs/ios-xe/"
    }
  },
  "servers": [
    {
      "url": "https://{device}:{port}",
      "description": "IOS-XE Device RESTCONF API",
      "variables": {
        "device": {
          "default": "router.example.com",
          "description": "Device IP or hostname"
        },
        "port": {
          "default": "443",
          "description": "HTTPS port"
        }
      }
    }
  ],
  "paths": {
    "/data/Cisco-IOS-XE-native:native/vlan": {
      "get": {
        "summary": "Get vlan configuration",
        "description": "Retrieve all VLAN configurations",
        "operationId": "get-vlan",
        "tags": ["containers", "switching"],
        "responses": {
          "200": {"description": "Successful response"}
        }
      },
      "put": {"summary": "Configure VLANs", "operationId": "put-vlan", "tags": ["containers", "switching"], "requestBody": {"required": true, "content": {"application/yang-data+json": {"schema": {"type": "object"}}}}, "responses": {"204": {"description": "Successfully configured"}}},
      "patch": {"summary": "Update VLANs", "operationId": "patch-vlan", "tags": ["containers", "switching"], "requestBody": {"required": true, "content": {"application/yang-data+json": {"schema": {"type": "object"}}}}, "responses": {"204": {"description": "Successfully updated"}}},
      "delete": {"summary": "Delete VLANs", "operationId": "delete-vlan", "tags": ["containers", "switching"], "responses": {"204": {"description": "Successfully deleted"}}}
    },
    "/data/Cisco-IOS-XE-native:native/logging": {
      "get": {"summary": "Get logging configuration", "operationId": "get-logging", "tags": ["containers", "management"], "responses": {"200": {"description": "Successful response"}}},
      "put": {"summary": "Configure logging", "operationId": "put-logging", "tags": ["containers", "management"], "requestBody": {"required": true, "content": {"application/yang-data+json": {"schema": {"type": "object"}}}}, "responses": {"204": {"description": "Successfully configured"}}},
      "patch": {"summary": "Update logging", "operationId": "patch-logging", "tags": ["containers", "management"], "requestBody": {"required": true, "content": {"application/yang-data+json": {"schema": {"type": "object"}}}}, "responses": {"204": {"description": "Successfully updated"}}},
      "delete": {"summary": "Delete logging", "operationId": "delete-logging", "tags": ["containers", "management"], "responses": {"204": {"description": "Successfully deleted"}}}
