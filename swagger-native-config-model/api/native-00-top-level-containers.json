      "patch": {"summary": "Update spanning-tree", "operationId": "patch-spanning-tree", "tags": ["containers", "switching"], "requestBody": {"required": true, "content": {"application/yang-data+json": {"schema": {"type": "object"}}}}, "responses": {"204": {"description": "Successfully updated"}}},
      "delete": {"summary": "Delete spanning-tree", "operationId": "delete-spanning-tree", "tags": ["containers", "switching"], "responses": {"204": {"description": "Successfully deleted"}}}
