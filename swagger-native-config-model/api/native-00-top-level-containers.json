{
  "openapi": "3.0.0",
  "info": {
    "title": "Cisco IOS-XE Native Config - Top-Level Containers (Small/Miscellaneous)",
    "description": "Smaller top-level container nodes in the Cisco-IOS-XE-native model. Large/complex containers (ip, router, crypto, aaa, line, vrf, interface) have dedicated category pages.\n\n**What's included:** Small/medium containers: vlan, logging, snmp-server, nat, spanning-tree, policy, segment-routing, license, platform, service, controller, and others.\n\n**For larger containers, see dedicated pages:** IP, Router, Crypto, AAA, Line, VRF\n\n**Note:** Interface container excluded (too large for Swagger UI)\n\n**Related:** See 'Top-Level Leafs' for simple values (hostname, version, etc.)",
    "version": "17.18.1",
    "contact": {
      "name": "Cisco IOS-XE RESTCONF API",
      "url": "https://developer.cisco.com/docs/ios-xe/"
    }
  },
  "servers": [
    {
      "url": "https://{device}:{port}",
      "description": "IOS-XE Device RESTCONF API",
      "variables": {
        "device": {
          "default": "devnetsandboxiosxec9k.cisco.com",
          "description": "Device IP or hostname"
        },
        "port": {
          "default": "443",
          "description": "HTTPS port"
        }
      }
    }
  ],
  "paths": {
    "/data/Cisco-IOS-XE-native:native/vlan": {
      "get": {
        "summary": "Get vlan configuration",
        "description": "Retrieve all VLAN configurations",
        "operationId": "get-vlan",
        "tags": ["switching"],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/yang-data+json": {
                "schema": {"type": "object"},
                "example": {
                  "Cisco-IOS-XE-native:vlan": {
                    "vlan-list": [
                      {
                        "id": "10",
                        "name": "Data-VLAN"
                      },
                      {
                        "id": "20",
                        "name": "Voice-VLAN"
                      }
                    ]
                  }
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Configure VLANs",
        "operationId": "put-vlan",
        "tags": ["switching"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {"type": "object"},
              "example": {
                "Cisco-IOS-XE-native:vlan": {
                  "vlan-list": [
                    {
                      "id": "10",
                      "name": "Data-VLAN"
                    },
                    {
                      "id": "20",
                      "name": "Voice-VLAN"
                    }
                  ]
                }
              }
            }
          }
        },
        "responses": {"204": {"description": "Successfully configured"}}
      },
      "patch": {
        "summary": "Update VLANs",
        "operationId": "patch-vlan",
        "tags": ["switching"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {"type": "object"},
              "example": {
                "Cisco-IOS-XE-native:vlan": {
                  "vlan-list": [
                    {
                      "id": "10",
                      "name": "Data-VLAN"
                    }
                  ]
                }
              }
            }
          }
        },
        "responses": {"204": {"description": "Successfully updated"}}
      },
      "delete": {"summary": "Delete VLANs", "operationId": "delete-vlan", "tags": ["switching"], "responses": {"204": {"description": "Successfully deleted"}}}
    },
    "/data/Cisco-IOS-XE-native:native/logging": {
      "get": {"summary": "Get logging configuration", "operationId": "get-logging", "tags": ["management"], "responses": {"200": {"description": "Successful response", "content": {"application/yang-data+json": {"schema": {"type": "object"}, "example": {"Cisco-IOS-XE-native:logging": {"buffered": {"size": 32000, "severity": "informational"}, "console": {"severity": "warnings"}, "host": [{"ipv4-host": "192.0.2.10"}]}}}}}}},
      "put": {
        "summary": "Configure logging",
        "operationId": "put-logging",
        "tags": ["management"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {"type": "object"},
              "example": {
                "Cisco-IOS-XE-native:logging": {
                  "buffered": {
                    "size": 32000,
                    "severity": "informational"
                  },
                  "console": {
                    "severity": "warnings"
                  },
                  "host": [
                    {
                      "ipv4-host": "192.0.2.10"
                    }
                  ]
                }
              }
            }
          }
        },
        "responses": {"204": {"description": "Successfully configured"}}
      },
      "patch": {
        "summary": "Update logging",
        "operationId": "patch-logging",
        "tags": ["management"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {"type": "object"},
              "example": {
                "Cisco-IOS-XE-native:logging": {
                  "buffered": {
                    "size": 64000
                  }
                }
              }
            }
          }
        },
        "responses": {"204": {"description": "Successfully updated"}}
      },
      "delete": {"summary": "Delete logging", "operationId": "delete-logging", "tags": ["management"], "responses": {"204": {"description": "Successfully deleted"}}}
    },
    "/data/Cisco-IOS-XE-native:native/snmp-server": {
      "get": {"summary": "Get SNMP configuration", "operationId": "get-snmp-server", "tags": ["management"], "responses": {"200": {"description": "Successful response", "content": {"application/yang-data+json": {"schema": {"type": "object"}, "example": {"Cisco-IOS-XE-native:snmp-server": {"community": [{"name": "public", "permission": "RO"}], "location": "Building-1-Floor-2-Rack-3", "contact": "Network Operations <netops@example.com>"}}}}}}},
      "put": {
        "summary": "Configure SNMP",
        "operationId": "put-snmp-server",
        "tags": ["management"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {"type": "object"},
              "example": {
                "Cisco-IOS-XE-native:snmp-server": {
                  "community": [
                    {
                      "name": "public",
                      "permission": "RO"
                    }
                  ],
                  "location": "Building-1-Floor-2-Rack-3",
                  "contact": "Network Operations <netops@example.com>"
                }
              }
            }
          }
        },
        "responses": {"204": {"description": "Successfully configured"}}
      },
      "patch": {
        "summary": "Update SNMP",
        "operationId": "patch-snmp-server",
        "tags": ["management"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {"type": "object"},
              "example": {
                "Cisco-IOS-XE-native:snmp-server": {
                  "location": "DC1-Floor3-Rack5"
                }
              }
            }
          }
        },
        "responses": {"204": {"description": "Successfully updated"}}
      },
      "delete": {"summary": "Delete SNMP", "operationId": "delete-snmp-server", "tags": ["management"], "responses": {"204": {"description": "Successfully deleted"}}}
    },
    "/data/Cisco-IOS-XE-native:native/nat": {
      "get": {"summary": "Get NAT configuration", "operationId": "get-nat", "tags": ["services"], "responses": {"200": {"description": "Successful response", "content": {"application/yang-data+json": {"schema": {"type": "object"}, "example": {"Cisco-IOS-XE-native:nat": {"inside": {"source": {"list": [{"id": "1", "interface": "GigabitEthernet0/0/0", "overload": true}]}}}}}}}}},
      "put": {
        "summary": "Configure NAT",
        "operationId": "put-nat",
        "tags": ["services"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {"type": "object"},
              "example": {
                "Cisco-IOS-XE-native:nat": {
                  "inside": {
                    "source": {
                      "list": [
                        {
                          "id": "1",
                          "interface": "GigabitEthernet0/0/0",
                          "overload": true
                        }
                      ]
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {"204": {"description": "Successfully configured"}}
      },
      "patch": {
        "summary": "Update NAT",
        "operationId": "patch-nat",
        "tags": ["services"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {"type": "object"},
              "example": {
                "Cisco-IOS-XE-native:nat": {
                  "translation": {
                    "timeout": 3600
                  }
                }
              }
            }
          }
        },
        "responses": {"204": {"description": "Successfully updated"}}
      },
      "delete": {"summary": "Delete NAT", "operationId": "delete-nat", "tags": ["services"], "responses": {"204": {"description": "Successfully deleted"}}}
    },
    "/data/Cisco-IOS-XE-native:native/spanning-tree": {
      "get": {"summary": "Get spanning-tree configuration", "operationId": "get-spanning-tree", "tags": ["switching"], "responses": {"200": {"description": "Successful response", "content": {"application/yang-data+json": {"schema": {"type": "object"}, "example": {"Cisco-IOS-XE-native:spanning-tree": {"mode": "rapid-pvst", "extend": {"system-id": [null]}, "vlan": [{"id": "10", "priority": 4096}]}}}}}}},
      "put": {
        "summary": "Configure spanning-tree",
        "operationId": "put-spanning-tree",
        "tags": ["switching"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {"type": "object"},
              "example": {
                "Cisco-IOS-XE-native:spanning-tree": {
                  "mode": "rapid-pvst",
                  "extend": {
                    "system-id": [null]
                  },
                  "vlan": [
                    {
                      "id": "10",
                      "priority": 4096
                    }
                  ]
                }
              }
            }
          }
        },
        "responses": {"204": {"description": "Successfully configured"}}
      },
      "patch": {
        "summary": "Update spanning-tree",
        "operationId": "patch-spanning-tree",
        "tags": ["switching"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {"type": "object"},
              "example": {
                "Cisco-IOS-XE-native:spanning-tree": {
                  "vlan": [
                    {
                      "id": "10",
                      "priority": 8192
                    }
                  ]
                }
              }
            }
          }
        },
        "responses": {"204": {"description": "Successfully updated"}}
      },
      "delete": {"summary": "Delete spanning-tree", "operationId": "delete-spanning-tree", "tags": ["switching"], "responses": {"204": {"description": "Successfully deleted"}}}
    }
  },
  "components": {
    "securitySchemes": {
      "basicAuth": {
        "type": "http",
        "scheme": "basic"
      }
    }
  },
  "security": [{"basicAuth": []}],
  "tags": [
    {"name": "switching", "description": "Layer 2 switching configurations"},
    {"name": "management", "description": "Device management"},
    {"name": "services", "description": "Network services"}
  ]
}
