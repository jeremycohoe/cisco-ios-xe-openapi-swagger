{
  "openapi": "3.0.0",
  "info": {
    "title": "Cisco IOS-XE Native Config - Top-Level Leafs (Simple Values)",
    "description": "All top-level leaf nodes in the Cisco-IOS-XE-native model. These are simple configuration values like hostname, version, config-register, etc. Fast and lightweight - perfect for basic device configuration.\n\n**What's included:** All simple string/boolean/numeric values at /native root level.\n\n**Related:** See 'Top-Level Containers' for complex structures (ip, interface, router, etc.)",
    "version": "17.18.1",
    "contact": {
      "name": "Cisco IOS-XE RESTCONF API",
      "url": "https://developer.cisco.com/docs/ios-xe/"
    }
  },
  "servers": [
    {
      "url": "https://{device}:{port}",
      "description": "IOS-XE Device RESTCONF API",
      "variables": {
        "device": {
          "default": "router.example.com",
          "description": "Device IP or hostname"
        },
        "port": {
          "default": "443",
          "description": "HTTPS port"
        }
      }
    }
  ],
  "paths": {
    "/data/Cisco-IOS-XE-native:native/hostname": {
      "get": {
        "summary": "Get hostname",
        "description": "Retrieve the device's network hostname",
        "operationId": "get-hostname",
        "tags": ["leafs", "system-basics"],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "Cisco-IOS-XE-native:hostname": {
                      "type": "string",
                      "pattern": "^[a-zA-Z0-9]([a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?$",
                      "description": "Device hostname (1-63 characters)"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Set hostname",
        "description": "Configure the device hostname",
        "operationId": "put-hostname",
        "tags": ["leafs", "system-basics"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "Cisco-IOS-XE-native:hostname": {
                    "type": "string",
                    "pattern": "^[a-zA-Z0-9]([a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?$"
                  }
                }
              },
              "example": {
                "Cisco-IOS-XE-native:hostname": "router-core-01"
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully configured" }
        }
      },
      "patch": {
        "summary": "Update hostname",
        "description": "Merge update to hostname configuration",
        "operationId": "patch-hostname",
        "tags": ["leafs", "system-basics"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "Cisco-IOS-XE-native:hostname": { "type": "string" }
                }
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully updated" }
        }
      },
      "delete": {
        "summary": "Delete hostname",
        "description": "Remove hostname configuration (reverts to default)",
        "operationId": "delete-hostname",
        "tags": ["leafs", "system-basics"],
        "responses": {
          "204": { "description": "Successfully deleted" }
        }
      }
    },
