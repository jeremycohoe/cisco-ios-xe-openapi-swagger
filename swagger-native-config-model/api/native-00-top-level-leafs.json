        }
      }
    },
    "/data/Cisco-IOS-XE-native:native/config-register": {
      "get": {
        "summary": "Get config-register",
        "description": "Retrieve configuration register value",
        "operationId": "get-config-register",
        "tags": ["leafs", "boot"],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "Cisco-IOS-XE-native:config-register": {
                      "type": "string",
                      "pattern": "^0x[0-9A-Fa-f]{4}$",
                      "description": "Config register (e.g., 0x2102)"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Set config-register",
        "description": "Configure the boot configuration register",
        "operationId": "put-config-register",
        "tags": ["leafs", "boot"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "Cisco-IOS-XE-native:config-register": {
                    "type": "string",
                    "pattern": "^0x[0-9A-Fa-f]{4}$"
                  }
                }
              },
              "example": {
                "Cisco-IOS-XE-native:config-register": "0x2102"
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully configured" }
        }
      },
      "delete": {
        "summary": "Delete config-register",
        "description": "Remove config-register configuration",
        "operationId": "delete-config-register",
        "tags": ["leafs", "boot"],
        "responses": {
          "204": { "description": "Successfully deleted" }
        }
      }
    },
