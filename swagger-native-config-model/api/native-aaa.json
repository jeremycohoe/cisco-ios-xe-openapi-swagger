{
  "openapi": "3.0.0",
  "info": {
    "title": "Cisco IOS-XE Native Config - AAA (Authentication, Authorization, Accounting)",
    "description": "AAA configuration for Cisco-IOS-XE-native model. This container includes authentication, authorization, and accounting configurations for device access control.\n\n**What's included:** Complete AAA configuration hierarchy under /native/aaa\n\n**Common use cases:** RADIUS/TACACS+ authentication, command authorization, accounting, login banners, method lists\n\n**Related:** See 'TACACS' and 'RADIUS' for server configuration, 'Line' for access line config",
    "version": "17.18.1",
    "contact": {
      "name": "Cisco IOS-XE RESTCONF API",
      "url": "https://developer.cisco.com/docs/ios-xe/"
    }
  },
  "servers": [
    {
      "url": "https://{device}:{port}",
      "description": "IOS-XE Device RESTCONF API",
      "variables": {
        "device": {
          "default": "router.example.com",
          "description": "Device IP or hostname"
        },
        "port": {
          "default": "443",
          "description": "HTTPS port"
        }
      }
    }
  ],
  "paths": {
    "/data/Cisco-IOS-XE-native:native/aaa": {
      "get": {
        "summary": "Get AAA configuration",
        "description": "Retrieve entire AAA configuration container",
        "operationId": "get-aaa-all",
        "tags": ["aaa-root"],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "Cisco-IOS-XE-native:aaa": {
                      "type": "object",
                      "description": "Complete AAA configuration"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Replace AAA configuration",
        "description": "Replace entire AAA configuration",
        "operationId": "put-aaa-all",
        "tags": ["aaa-root"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              },
              "example": {
                "Cisco-IOS-XE-native:aaa": {
                  "new-model": [null],
                  "authentication": {
                    "login": [
                      {
                        "name": "default",
                        "a1": {
                          "local": [null]
                        }
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully configured" }
        }
      },
      "patch": {
        "summary": "Update AAA configuration",
        "description": "Merge update to AAA configuration",
        "operationId": "patch-aaa-all",
        "tags": ["aaa-root"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              },
              "example": {
                "Cisco-IOS-XE-native:aaa": {
                  "session-id": "common"
                }
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully updated" }
        }
      },
      "delete": {
        "summary": "Delete AAA configuration",
        "description": "Remove all AAA configuration (WARNING: may lock out management access)",
        "operationId": "delete-aaa-all",
        "tags": ["aaa-root"],
        "responses": {
          "204": { "description": "Successfully deleted" }
        }
      }
    },
    "/data/Cisco-IOS-XE-native:native/aaa/authentication": {
      "get": {
        "summary": "Get authentication configuration",
        "description": "Retrieve authentication method lists",
        "operationId": "get-aaa-authentication",
        "tags": ["authentication"],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Configure authentication",
        "description": "Replace authentication configuration",
        "operationId": "put-aaa-authentication",
        "tags": ["authentication"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              },
              "example": {
                "Cisco-IOS-XE-aaa:authentication": {
                  "login": [
                    {
                      "name": "default",
                      "a1": {
                        "group": "tacacs+"
                      },
                      "a2": {
                        "local": [null]
                      }
                    },
                    {
                      "name": "CONSOLE",
                      "a1": {
                        "local": [null]
                      }
                    }
                  ]
                }
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully configured" }
        }
      },
      "patch": {
        "summary": "Update authentication configuration",
        "description": "Merge update to authentication configuration",
        "operationId": "patch-aaa-authentication",
        "tags": ["authentication"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              },
              "example": {
                "Cisco-IOS-XE-aaa:authentication": {
                  "enable": {
                    "default": {
                      "a1": {
                        "enable": [null]
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully updated" }
        }
      },
      "delete": {
        "summary": "Delete authentication configuration",
        "description": "Remove authentication configuration",
        "operationId": "delete-aaa-authentication",
        "tags": ["authentication"],
        "responses": {
          "204": { "description": "Successfully deleted" }
        }
      }
    },
    "/data/Cisco-IOS-XE-native:native/aaa/authorization": {
      "get": {
        "summary": "Get authorization configuration",
        "description": "Retrieve authorization method lists",
        "operationId": "get-aaa-authorization",
        "tags": ["authorization"],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Configure authorization",
        "description": "Replace authorization configuration",
        "operationId": "put-aaa-authorization",
        "tags": ["authorization"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              },
              "example": {
                "Cisco-IOS-XE-aaa:authorization": {
                  "exec": [
                    {
                      "name": "default",
                      "a1": {
                        "group": "tacacs+"
                      },
                      "a2": {
                        "local": [null]
                      }
                    }
                  ],
                  "commands": [
                    {
                      "level": 15,
                      "name": "default",
                      "a1": {
                        "group": "tacacs+"
                      }
                    }
                  ]
                }
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully configured" }
        }
      },
      "patch": {
        "summary": "Update authorization configuration",
        "description": "Merge update to authorization configuration",
        "operationId": "patch-aaa-authorization",
        "tags": ["authorization"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              },
              "example": {
                "Cisco-IOS-XE-aaa:authorization": {
                  "config-commands": [null]
                }
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully updated" }
        }
      },
      "delete": {
        "summary": "Delete authorization configuration",
        "description": "Remove authorization configuration",
        "operationId": "delete-aaa-authorization",
        "tags": ["authorization"],
        "responses": {
          "204": { "description": "Successfully deleted" }
        }
      }
    },
    "/data/Cisco-IOS-XE-native:native/aaa/accounting": {
      "get": {
        "summary": "Get accounting configuration",
        "description": "Retrieve accounting method lists",
        "operationId": "get-aaa-accounting",
        "tags": ["accounting"],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Configure accounting",
        "description": "Replace accounting configuration",
        "operationId": "put-aaa-accounting",
        "tags": ["accounting"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              },
              "example": {
                "Cisco-IOS-XE-aaa:accounting": {
                  "exec": [
                    {
                      "name": "default",
                      "start-stop": {
                        "group1": "tacacs+"
                      }
                    }
                  ],
                  "commands": [
                    {
                      "level": 15,
                      "name": "default",
                      "start-stop": {
                        "group1": "tacacs+"
                      }
                    }
                  ]
                }
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully configured" }
        }
      },
      "patch": {
        "summary": "Update accounting configuration",
        "description": "Merge update to accounting configuration",
        "operationId": "patch-aaa-accounting",
        "tags": ["accounting"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              },
              "example": {
                "Cisco-IOS-XE-aaa:accounting": {
                  "system": {
                    "guarantee-first": [null]
                  }
                }
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully updated" }
        }
      },
      "delete": {
        "summary": "Delete accounting configuration",
        "description": "Remove accounting configuration",
        "operationId": "delete-aaa-accounting",
        "tags": ["accounting"],
        "responses": {
          "204": { "description": "Successfully deleted" }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "basicAuth": {
        "type": "http",
