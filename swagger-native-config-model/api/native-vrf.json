      },
      "patch": {
        "summary": "Update VRF definitions",
        "description": "Merge update to VRF definition configurations",
        "operationId": "patch-vrf-definition",
        "tags": ["vrf-config"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully updated" }
        }
      },
      "delete": {
        "summary": "Delete VRF definitions",
        "description": "Remove VRF definition configurations",
        "operationId": "delete-vrf-definition",
        "tags": ["vrf-config"],
        "responses": {
          "204": { "description": "Successfully deleted" }
        }
      }
    }
