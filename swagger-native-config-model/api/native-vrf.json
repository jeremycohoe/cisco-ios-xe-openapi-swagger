{
  "openapi": "3.0.0",
  "info": {
    "title": "Cisco IOS-XE Native Config - VRF (Virtual Routing and Forwarding)",
    "description": "VRF configuration for Cisco-IOS-XE-native model. This container includes virtual routing and forwarding instance configurations for network segmentation.\n\n**What's included:** Complete VRF configuration hierarchy under /native/vrf\n\n**Common use cases:** Multi-tenancy, network segmentation, MPLS VPN, route isolation\n\n**Related:** See 'Router' for routing protocols, 'IP' for routing configuration",
    "version": "17.18.1",
    "contact": {
      "name": "Cisco IOS-XE RESTCONF API",
      "url": "https://developer.cisco.com/docs/ios-xe/"
    }
  },
  "servers": [
    {
      "url": "https://{device}:{port}",
      "description": "IOS-XE Device RESTCONF API",
      "variables": {
        "device": {
          "default": "router.example.com",
          "description": "Device IP or hostname"
        },
        "port": {
          "default": "443",
          "description": "HTTPS port"
        }
      }
    }
  ],
  "paths": {
    "/data/Cisco-IOS-XE-native:native/vrf": {
      "get": {
        "summary": "Get all VRF configuration",
        "description": "Retrieve all VRF definitions",
        "operationId": "get-vrf-all",
        "tags": ["vrf-root"],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "Cisco-IOS-XE-native:vrf": {
                      "type": "object",
                      "description": "Complete VRF configuration"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Replace VRF configuration",
        "description": "Replace all VRF definitions",
        "operationId": "put-vrf-all",
        "tags": ["vrf-root"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully configured" }
        }
      },
      "patch": {
        "summary": "Update VRF configuration",
        "description": "Merge update to VRF definitions",
        "operationId": "patch-vrf-all",
        "tags": ["vrf-root"],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "204": { "description": "Successfully updated" }
        }
      },
      "delete": {
        "summary": "Delete VRF configuration",
        "description": "Remove all VRF definitions",
        "operationId": "delete-vrf-all",
        "tags": ["vrf-root"],
        "responses": {
          "204": { "description": "Successfully deleted" }
        }
      }
    },
    "/data/Cisco-IOS-XE-native:native/vrf/definition": {
      "get": {
        "summary": "Get VRF definitions",
        "description": "Retrieve all VRF definition configurations",
        "operationId": "get-vrf-definition",
        "tags": ["vrf-config"],
