{
  "openapi": "3.0.0",
  "info": {
    "title": "CISCO-PORT-SECURITY-MIB SNMP Notifications",
    "description": "SNMP Trap notifications from CISCO-PORT-SECURITY-MIB for port security events",
    "version": "1.0.0"
  },
  "servers": [{"url": "https://{device}", "variables": {"device": {"default": "localhost"}}}],
  "security": [{"basicAuth": []}],
  "paths": {
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-PORT-SECURITY-MIB:cpsSecureMacAddrViolation": {
      "get": {
        "summary": "SNMP Trap: Port security MAC address violation",
        "description": "Notification when unauthorized MAC address detected on secure port",
        "operationId": "get_cpsSecureMacAddrViolation",
        "tags": ["Port Security Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-PORT-SECURITY-MIB:cpsInterfaceSecureUp": {
      "get": {
        "summary": "SNMP Trap: Port security interface enabled",
        "description": "Notification when port security is enabled on an interface",
        "operationId": "get_cpsInterfaceSecureUp",
        "tags": ["Port Security Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-PORT-SECURITY-MIB:cpsInterfaceSecureDown": {
      "get": {
        "summary": "SNMP Trap: Port security interface disabled",
        "description": "Notification when port security is disabled on an interface",
        "operationId": "get_cpsInterfaceSecureDown",
        "tags": ["Port Security Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-PORT-SECURITY-MIB:cpsIfVlanSecureUp": {
      "get": {
        "summary": "SNMP Trap: Port security enabled on VLAN",
        "description": "Notification when port security is enabled on a VLAN",
        "operationId": "get_cpsIfVlanSecureUp",
        "tags": ["Port Security Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-PORT-SECURITY-MIB:cpsIfVlanSecureDown": {
      "get": {
        "summary": "SNMP Trap: Port security disabled on VLAN",
        "description": "Notification when port security is disabled on a VLAN",
        "operationId": "get_cpsIfVlanSecureDown",
        "tags": ["Port Security Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-PORT-SECURITY-MIB:cpsIfVlanSecureMacAddrViolation": {
      "get": {
        "summary": "SNMP Trap: Port security VLAN MAC address violation",
        "description": "Notification when MAC address violation occurs on a secure VLAN",
        "operationId": "get_cpsIfVlanSecureMacAddrViolation",
        "tags": ["Port Security Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-PORT-SECURITY-MIB:cpsGlobalPortSecurityModeChange": {
      "get": {
        "summary": "SNMP Trap: Global port security mode change",
        "description": "Notification when global port security mode changes",
        "operationId": "get_cpsGlobalPortSecurityModeChange",
        "tags": ["Port Security Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-PORT-SECURITY-MIB:cpsIfSecurityViolationAction": {
      "get": {
        "summary": "SNMP Trap: Port security violation action triggered",
        "description": "Notification when security violation action is triggered on a port",
        "operationId": "get_cpsIfSecurityViolationAction",
        "tags": ["Port Security Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    }
  },
  "components": {
    "securitySchemes": {"basicAuth": {"type": "http", "scheme": "basic"}},
    "parameters": {
      "SubscriptionId": {"name": "id", "in": "path", "required": true, "schema": {"type": "string"}, "description": "Subscription identifier"},
      "ReceiverName": {"name": "name", "in": "path", "required": true, "schema": {"type": "string"}, "description": "Receiver name"}
    },
    "responses": {
      "NotificationResponse": {
        "description": "SNMP Trap notification data",
        "content": {
          "application/yang-data+json": {
            "schema": {
              "type": "object",
              "properties": {
                "notification": {
                  "type": "object",
                  "description": "Notification event data with timestamp and trap parameters"
                }
              }
            }
          }
        }
      }
    }
  }
}
