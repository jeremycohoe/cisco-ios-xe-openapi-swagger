{
  "openapi": "3.0.0",
  "info": {
    "title": "CISCO-IPSEC-FLOW-MONITOR-MIB SNMP Notifications",
    "description": "SNMP Trap notifications from CISCO-IPSEC-FLOW-MONITOR-MIB for IPsec VPN and IKE events",
    "version": "1.0.0"
  },
  "servers": [{"url": "https://{device}", "variables": {"device": {"default": "devnetsandboxiosxec9k.cisco.com"}}}],
  "security": [{"basicAuth": []}],
  "paths": {
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-IPSEC-FLOW-MONITOR-MIB:cikeTunnelStart": {
      "get": {
        "summary": "SNMP Trap: IKE tunnel started",
        "description": "Notification when an IKE tunnel is established",
        "operationId": "get_cikeTunnelStart",
        "tags": ["IKE Tunnel Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-IPSEC-FLOW-MONITOR-MIB:cikeTunnelStop": {
      "get": {
        "summary": "SNMP Trap: IKE tunnel stopped",
        "description": "Notification when an IKE tunnel is terminated",
        "operationId": "get_cikeTunnelStop",
        "tags": ["IKE Tunnel Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-IPSEC-FLOW-MONITOR-MIB:cikeSysFailure": {
      "get": {
        "summary": "SNMP Trap: IKE system failure",
        "description": "Notification when an IKE system failure occurs",
        "operationId": "get_cikeSysFailure",
        "tags": ["IKE Error Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-IPSEC-FLOW-MONITOR-MIB:cikeCertCrlFailure": {
      "get": {
        "summary": "SNMP Trap: IKE certificate/CRL failure",
        "description": "Notification when an IKE certificate or CRL validation failure occurs",
        "operationId": "get_cikeCertCrlFailure",
        "tags": ["IKE Security Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-IPSEC-FLOW-MONITOR-MIB:cikeProtocolFailure": {
      "get": {
        "summary": "SNMP Trap: IKE protocol failure",
        "description": "Notification when an IKE protocol failure occurs",
        "operationId": "get_cikeProtocolFailure",
        "tags": ["IKE Error Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-IPSEC-FLOW-MONITOR-MIB:cikeNoSa": {
      "get": {
        "summary": "SNMP Trap: IKE no SA available",
        "description": "Notification when no IKE SA is available for a peer",
        "operationId": "get_cikeNoSa",
        "tags": ["IKE SA Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-IPSEC-FLOW-MONITOR-MIB:cipSecTunnelStart": {
      "get": {
        "summary": "SNMP Trap: IPsec tunnel started",
        "description": "Notification when an IPsec tunnel is established",
        "operationId": "get_cipSecTunnelStart",
        "tags": ["IPsec Tunnel Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-IPSEC-FLOW-MONITOR-MIB:cipSecTunnelStop": {
      "get": {
        "summary": "SNMP Trap: IPsec tunnel stopped",
        "description": "Notification when an IPsec tunnel is terminated",
        "operationId": "get_cipSecTunnelStop",
        "tags": ["IPsec Tunnel Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-IPSEC-FLOW-MONITOR-MIB:cipSecSysFailure": {
      "get": {
        "summary": "SNMP Trap: IPsec system failure",
        "description": "Notification when an IPsec system failure occurs",
        "operationId": "get_cipSecSysFailure",
        "tags": ["IPsec Error Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-IPSEC-FLOW-MONITOR-MIB:cipSecSetUpFailure": {
      "get": {
        "summary": "SNMP Trap: IPsec setup failure",
        "description": "Notification when IPsec tunnel setup fails",
        "operationId": "get_cipSecSetUpFailure",
        "tags": ["IPsec Error Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-IPSEC-FLOW-MONITOR-MIB:cipSecEarlyTunTerm": {
      "get": {
        "summary": "SNMP Trap: IPsec early tunnel termination",
        "description": "Notification when an IPsec tunnel is terminated prematurely",
        "operationId": "get_cipSecEarlyTunTerm",
        "tags": ["IPsec Tunnel Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-IPSEC-FLOW-MONITOR-MIB:cipSecProtocolFailure": {
      "get": {
        "summary": "SNMP Trap: IPsec protocol failure",
        "description": "Notification when an IPsec protocol failure occurs",
        "operationId": "get_cipSecProtocolFailure",
        "tags": ["IPsec Error Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-IPSEC-FLOW-MONITOR-MIB:cipSecNoSa": {
      "get": {
        "summary": "SNMP Trap: IPsec no SA available",
        "description": "Notification when no IPsec SA is available",
        "operationId": "get_cipSecNoSa",
        "tags": ["IPsec SA Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    }
  },
  "components": {
    "securitySchemes": {"basicAuth": {"type": "http", "scheme": "basic"}},
    "parameters": {
      "SubscriptionId": {"name": "id", "in": "path", "required": true, "schema": {"type": "string"}, "description": "Subscription identifier"},
      "ReceiverName": {"name": "name", "in": "path", "required": true, "schema": {"type": "string"}, "description": "Receiver name"}
    },
    "responses": {
      "NotificationResponse": {
        "description": "SNMP Trap notification data",
        "content": {
          "application/yang-data+json": {
            "schema": {
              "type": "object",
              "properties": {
                "notification": {
                  "type": "object",
                  "description": "Notification event data with timestamp and trap parameters"
                }
              }
            }
          }
        }
      }
    }
  }
}
