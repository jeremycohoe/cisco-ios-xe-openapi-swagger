        "description": "Establishes WebSocket connection for Umbrella anycast server switch notifications",
        "tags": [
          "umbrella-events"
        ],
        "responses": {
          "101": {
            "description": "Switching Protocols - WebSocket connection established",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "$ref": "#/components/schemas/umbrella-anycast-server-switch"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/ws/event-streams/Cisco-IOS-XE-umbrella-oper-dp/umbrella-max-cft-flows": {
      "get": {
        "summary": "Subscribe to umbrella-max-cft-flows events",
