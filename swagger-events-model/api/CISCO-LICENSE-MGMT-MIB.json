{
  "openapi": "3.0.0",
  "info": {
    "title": "CISCO-LICENSE-MGMT-MIB SNMP Notifications",
    "description": "SNMP Trap notifications from CISCO-LICENSE-MGMT-MIB for software licensing events",
    "version": "1.0.0"
  },
  "servers": [{"url": "https://{device}", "variables": {"device": {"default": "devnetsandboxiosxec9k.cisco.com"}}}],
  "security": [{"basicAuth": []}],
  "paths": {
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-LICENSE-MGMT-MIB:clmgmtLicenseExpired": {
      "get": {
        "summary": "SNMP Trap: License expired",
        "description": "Notification when a license has expired",
        "operationId": "get_clmgmtLicenseExpired",
        "tags": ["License Expiry Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-LICENSE-MGMT-MIB:clmgmtLicenseExpiryWarning": {
      "get": {
        "summary": "SNMP Trap: License expiry warning",
        "description": "Notification when a license is approaching expiry",
        "operationId": "get_clmgmtLicenseExpiryWarning",
        "tags": ["License Expiry Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-LICENSE-MGMT-MIB:clmgmtLicenseUsageCountExceeded": {
      "get": {
        "summary": "SNMP Trap: License usage count exceeded",
        "description": "Notification when license usage count limit is exceeded",
        "operationId": "get_clmgmtLicenseUsageCountExceeded",
        "tags": ["License Usage Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-LICENSE-MGMT-MIB:clmgmtLicenseUsageCountAboutToExceed": {
      "get": {
        "summary": "SNMP Trap: License usage count about to exceed",
        "description": "Notification when license usage count is approaching limit",
        "operationId": "get_clmgmtLicenseUsageCountAboutToExceed",
        "tags": ["License Usage Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-LICENSE-MGMT-MIB:clmgmtLicenseInstalled": {
      "get": {
        "summary": "SNMP Trap: License installed",
        "description": "Notification when a new license is installed",
        "operationId": "get_clmgmtLicenseInstalled",
        "tags": ["License Management Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-LICENSE-MGMT-MIB:clmgmtLicenseCleared": {
      "get": {
        "summary": "SNMP Trap: License cleared",
        "description": "Notification when a license is cleared from the device",
        "operationId": "get_clmgmtLicenseCleared",
        "tags": ["License Management Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-LICENSE-MGMT-MIB:clmgmtLicenseRevoked": {
      "get": {
        "summary": "SNMP Trap: License revoked",
        "description": "Notification when a license is revoked",
        "operationId": "get_clmgmtLicenseRevoked",
        "tags": ["License Management Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-LICENSE-MGMT-MIB:clmgmtLicenseEULAAccepted": {
      "get": {
        "summary": "SNMP Trap: License EULA accepted",
        "description": "Notification when license End User License Agreement is accepted",
        "operationId": "get_clmgmtLicenseEULAAccepted",
        "tags": ["License Compliance Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-LICENSE-MGMT-MIB:clmgmtLicenseNotEnforced": {
      "get": {
        "summary": "SNMP Trap: License not enforced",
        "description": "Notification when license enforcement is disabled for a feature",
        "operationId": "get_clmgmtLicenseNotEnforced",
        "tags": ["License Compliance Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-LICENSE-MGMT-MIB:clmgmtLicenseSubscriptionExpiryWarning": {
      "get": {
        "summary": "SNMP Trap: License subscription expiry warning",
        "description": "Notification when a subscription-based license is approaching expiry",
        "operationId": "get_clmgmtLicenseSubscriptionExpiryWarning",
        "tags": ["License Subscription Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-LICENSE-MGMT-MIB:clmgmtLicenseSubscriptionExtExpiryWarning": {
      "get": {
        "summary": "SNMP Trap: License subscription extension expiry warning",
        "description": "Notification when a subscription extension is approaching expiry",
        "operationId": "get_clmgmtLicenseSubscriptionExtExpiryWarning",
        "tags": ["License Subscription Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-LICENSE-MGMT-MIB:clmgmtLicenseSubscriptionExpired": {
      "get": {
        "summary": "SNMP Trap: License subscription expired",
        "description": "Notification when a subscription-based license has expired",
        "operationId": "get_clmgmtLicenseSubscriptionExpired",
        "tags": ["License Subscription Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-LICENSE-MGMT-MIB:clmgmtLicenseDeploymentStatusChange": {
      "get": {
        "summary": "SNMP Trap: License deployment status change",
        "description": "Notification when license deployment status changes",
        "operationId": "get_clmgmtLicenseDeploymentStatusChange",
        "tags": ["License Management Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-LICENSE-MGMT-MIB:clmgmtLicenseError": {
      "get": {
        "summary": "SNMP Trap: License error",
        "description": "Notification when a license error condition occurs",
        "operationId": "get_clmgmtLicenseError",
        "tags": ["License Error Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    }
  },
  "components": {
    "securitySchemes": {"basicAuth": {"type": "http", "scheme": "basic"}},
    "parameters": {
      "SubscriptionId": {"name": "id", "in": "path", "required": true, "schema": {"type": "string"}, "description": "Subscription identifier"},
      "ReceiverName": {"name": "name", "in": "path", "required": true, "schema": {"type": "string"}, "description": "Receiver name"}
    },
    "responses": {
      "NotificationResponse": {
        "description": "SNMP Trap notification data",
        "content": {
          "application/yang-data+json": {
            "schema": {
              "type": "object",
              "properties": {
                "notification": {
                  "type": "object",
                  "description": "Notification event data with timestamp and trap parameters"
                }
              }
            }
          }
        }
      }
    }
  }
}
