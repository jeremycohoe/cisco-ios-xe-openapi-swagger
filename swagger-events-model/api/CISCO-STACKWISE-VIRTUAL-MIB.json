{
  "openapi": "3.0.0",
  "info": {
    "title": "CISCO-STACKWISE-VIRTUAL-MIB SNMP Notifications",
    "description": "SNMP Trap notifications from CISCO-STACKWISE-VIRTUAL-MIB for virtual stacking events",
    "version": "1.0.0"
  },
  "servers": [{"url": "https://{device}", "variables": {"device": {"default": "localhost"}}}],
  "security": [{"basicAuth": []}],
  "paths": {
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-STACKWISE-VIRTUAL-MIB:csvDualActiveDetection": {
      "get": {
        "summary": "SNMP Trap: StackWise Virtual dual-active detection",
        "description": "Notification when dual-active condition is detected in StackWise Virtual setup",
        "operationId": "get_csvDualActiveDetection",
        "tags": ["StackWise Virtual Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-STACKWISE-VIRTUAL-MIB:csvStackwiseVirtualLinkChange": {
      "get": {
        "summary": "SNMP Trap: StackWise Virtual link status change",
        "description": "Notification when StackWise Virtual link status changes",
        "operationId": "get_csvStackwiseVirtualLinkChange",
        "tags": ["StackWise Virtual Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-STACKWISE-VIRTUAL-MIB:csvSwitchAdded": {
      "get": {
        "summary": "SNMP Trap: StackWise Virtual switch added",
        "description": "Notification when a switch is added to StackWise Virtual domain",
        "operationId": "get_csvSwitchAdded",
        "tags": ["StackWise Virtual Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-STACKWISE-VIRTUAL-MIB:csvSwitchRemoved": {
      "get": {
        "summary": "SNMP Trap: StackWise Virtual switch removed",
        "description": "Notification when a switch is removed from StackWise Virtual domain",
        "operationId": "get_csvSwitchRemoved",
        "tags": ["StackWise Virtual Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-STACKWISE-VIRTUAL-MIB:csvConvertModeChange": {
      "get": {
        "summary": "SNMP Trap: StackWise Virtual convert mode change",
        "description": "Notification when StackWise Virtual mode conversion occurs",
        "operationId": "get_csvConvertModeChange",
        "tags": ["StackWise Virtual Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-STACKWISE-VIRTUAL-MIB:csvStackMasterChange": {
      "get": {
        "summary": "SNMP Trap: StackWise Virtual master change",
        "description": "Notification when StackWise Virtual active switch changes",
        "operationId": "get_csvStackMasterChange",
        "tags": ["StackWise Virtual Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-STACKWISE-VIRTUAL-MIB:csvLinkBandwidthChange": {
      "get": {
        "summary": "SNMP Trap: StackWise Virtual link bandwidth change",
        "description": "Notification when StackWise Virtual link bandwidth changes",
        "operationId": "get_csvLinkBandwidthChange",
        "tags": ["StackWise Virtual Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-STACKWISE-VIRTUAL-MIB:csvVersionMismatch": {
      "get": {
        "summary": "SNMP Trap: StackWise Virtual version mismatch",
        "description": "Notification when software version mismatch detected in StackWise Virtual setup",
        "operationId": "get_csvVersionMismatch",
        "tags": ["StackWise Virtual Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-STACKWISE-VIRTUAL-MIB:csvConfigMismatch": {
      "get": {
        "summary": "SNMP Trap: StackWise Virtual configuration mismatch",
        "description": "Notification when configuration mismatch detected in StackWise Virtual setup",
        "operationId": "get_csvConfigMismatch",
        "tags": ["StackWise Virtual Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-STACKWISE-VIRTUAL-MIB:csvDomainMismatch": {
      "get": {
        "summary": "SNMP Trap: StackWise Virtual domain mismatch",
        "description": "Notification when domain mismatch detected in StackWise Virtual setup",
        "operationId": "get_csvDomainMismatch",
        "tags": ["StackWise Virtual Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-STACKWISE-VIRTUAL-MIB:csvDadInterfaceChange": {
      "get": {
        "summary": "SNMP Trap: StackWise Virtual DAD interface change",
        "description": "Notification when Dual-Active Detection interface status changes",
        "operationId": "get_csvDadInterfaceChange",
        "tags": ["StackWise Virtual Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-STACKWISE-VIRTUAL-MIB:csvRedundancyStateChange": {
      "get": {
        "summary": "SNMP Trap: StackWise Virtual redundancy state change",
        "description": "Notification when StackWise Virtual redundancy state changes",
        "operationId": "get_csvRedundancyStateChange",
        "tags": ["StackWise Virtual Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
