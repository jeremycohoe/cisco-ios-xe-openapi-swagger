{
  "openapi": "3.0.0",
  "info": {
    "title": "CISCO-OSPF-TRAP-MIB SNMP Notifications",
    "description": "SNMP Trap notifications from CISCO-OSPF-TRAP-MIB for Cisco OSPF routing protocol extensions",
    "version": "1.0.0"
  },
  "servers": [{"url": "https://{device}", "variables": {"device": {"default": "localhost"}}}],
  "security": [{"basicAuth": []}],
  "paths": {
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-OSPF-TRAP-MIB:cospfIfConfigError": {
      "get": {
        "summary": "SNMP Trap: Cisco OSPF interface configuration error",
        "description": "Notification when OSPF interface configuration error detected (Cisco extension)",
        "operationId": "get_cospfIfConfigError",
        "tags": ["OSPF Interface Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-OSPF-TRAP-MIB:cospfVirtIfConfigError": {
