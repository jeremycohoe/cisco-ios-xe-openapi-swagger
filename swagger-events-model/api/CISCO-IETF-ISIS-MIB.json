{
  "openapi": "3.0.0",
  "info": {
    "title": "CISCO-IETF-ISIS-MIB SNMP Notifications",
    "description": "SNMP Trap notifications from CISCO-IETF-ISIS-MIB for ISIS routing protocol events",
    "version": "1.0.0"
  },
  "servers": [{"url": "https://{device}", "variables": {"device": {"default": "devnetsandboxiosxec9k.cisco.com"}}}],
  "security": [{"basicAuth": []}],
  "paths": {
    "/data/ietf-subscribed-notifications:subscriptions/subscription={id}/receivers/receiver={name}/CISCO-IETF-ISIS-MIB:ciiDatabaseOverload": {
      "get": {
        "summary": "SNMP Trap: ISIS database overload",
        "description": "Notification when ISIS database enters or exits overload state",
        "operationId": "get_ciiDatabaseOverload",
        "tags": ["ISIS Database Events"],
        "parameters": [{"$ref": "#/components/parameters/SubscriptionId"}, {"$ref": "#/components/parameters/ReceiverName"}],
        "responses": {"200": {"$ref": "#/components/responses/NotificationResponse"}}
      }
    },
