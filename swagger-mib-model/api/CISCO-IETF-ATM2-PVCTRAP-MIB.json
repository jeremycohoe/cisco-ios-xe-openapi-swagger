              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "description": "A table indicating all VCLs for which there is an\n        active row in the atmVclTable having an atmVclConnKind\n        value of `pvc' and an atmVclOperStatus with a value\n        other than `up'.",
                  "properties": {
                    "atmCurrentlyFailingPVclEntry": {
                      "type": "array",
                      "description": "Each entry in this table represents a VCL for which\n          the atmVclRowStatus is `active', the atmVclConnKind is\n          `pvc', and the atmVclOperStatus is other than `up'.",
                      "items": {
                        "type": "object",
                        "properties": {
                          "ifIndex": {
                            "type": "string",
                            "description": "ifIndex",
                            "x-yang-type": "leafref"
                          },
                          "atmVclVpi": {
                            "type": "string",
                            "description": "atmVclVpi",
                            "x-yang-type": "leafref"
                          },
                          "atmVclVci": {
                            "type": "string",
                            "description": "atmVclVci",
                            "x-yang-type": "leafref"
                          },
                          "atmCurrentlyFailingPVclTimeStamp": {
                            "type": "string",
                            "description": "The time at which this PVCL began to fail.",
                            "x-yang-type": "yang:timestamp"
                          },
                          "atmPreviouslyFailedPVclTimeStamp": {
                            "type": "string",
                            "description": "The time at which this PVCL began to fail\n            during the PVC Notification interval.",
                            "x-yang-type": "yang:timestamp"
                          }
                        }
                      }
                    }
                  }
                },
                "example": {
                  "CISCO-IETF-ATM2-PVCTRAP-MIB:atmCurrentlyFailingPVclTable": {
                    "atmCurrentlyFailingPVclEntry": [
                      {
                        "ifIndex": "example-string",
