        "tags": [
          "Cisco-IOS-XE-utd-actions-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "utd_actions_rpc:utd-custom-signature-apply": {
                    "type": "object",
                    "properties": {
                      "profile-name": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 127,
                        "description": "Custom-signature profile name"
                      },
                      "filename": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 127,
                        "description": "Custom-signature file name"
                      }
                    },
                    "required": [
                      "profile-name"
                    ]
                  }
                }
              },
              "example": {
                "utd_actions_rpc:utd-custom-signature-apply": {
                  "profile-name": "flash:/config.txt",
                  "filename": "flash:/config.txt"
                }
              }
