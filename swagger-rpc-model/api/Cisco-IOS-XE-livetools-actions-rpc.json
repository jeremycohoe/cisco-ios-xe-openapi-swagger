        "tags": [
          "Cisco-IOS-XE-livetools-actions-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "livetools_actions_rpc:wol-action": {
                    "type": "object",
                    "properties": {
                      "mac-addr": {
                        "type": "string",
                        "description": "Destination MAC address"
                      },
                      "vlan": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 4094,
                        "description": "Destination VLAN identifier",
                        "default": "1"
                      },
                      "interval": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 3600,
                        "description": "Interval between retries",
                        "default": "120"
                      },
                      "count": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 255,
                        "description": "Number of packets to be sent",
                        "default": "1"
                      }
                    },
                    "required": [
                      "mac-addr"
                    ]
                  }
                }
              },
              "example": {
                "livetools_actions_rpc:wol-action": {
                  "mac-addr": "00:11:22:33:44:55",
                  "vlan": 100,
                  "interval": 10,
                  "count": 10
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
