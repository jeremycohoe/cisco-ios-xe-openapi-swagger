    "/operations/chassis_rpc:set-chassis-priority": {
      "post": {
        "summary": "Set chassis priority RPC.\n       Chassis with higher priority will become active module\n       on next reboot of active and standby modules.",
        "description": "Set chassis priority RPC.\n       Chassis with higher priority will become active module\n       on next reboot of active and standby modules.\n\n**Endpoint:** `POST /restconf/operations/chassis_rpc:set-chassis-priority`",
        "operationId": "set-chassis-priority",
        "tags": [
          "Cisco-IOS-XE-chassis-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "chassis_rpc:set-chassis-priority": {
                    "type": "object",
                    "properties": {
                      "number": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 2,
                        "description": "Chassis number"
                      },
                      "priority": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 2,
                        "description": "Chassis priority"
                      }
                    },
                    "required": [
                      "number",
                      "priority"
                    ]
                  }
                }
              },
              "example": {
                "chassis_rpc:set-chassis-priority": {
                  "number": 2,
                  "priority": 2
                }
              }
            }
          }
        },
