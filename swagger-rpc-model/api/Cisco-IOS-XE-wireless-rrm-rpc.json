    "/operations/wireless_rrm_rpc:rrm-rldr-set": {
      "post": {
        "summary": "Remote Leader Set Remote Control Procedure",
        "description": "Remote Leader Set Remote Control Procedure\n\n**Endpoint:** `POST /restconf/operations/wireless_rrm_rpc:rrm-rldr-set`",
        "operationId": "rrm-rldr-set",
        "tags": [
          "Cisco-IOS-XE-wireless-rrm-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "wireless_rrm_rpc:rrm-rldr-set": {
                    "type": "object",
                    "properties": {
                      "ip-addr": {
                        "type": "string",
                        "description": "Remote Leader IP Address"
                      },
                      "leader-name": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 32,
                        "pattern": "[!-~]([ -~]*[!-~])?",
                        "description": "Remote Leader Name"
                      },
                      "rf-network-name": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 20,
                        "pattern": "[!-~]([ -~]*[!-~])?",
                        "description": "RF Network Name"
                      },
                      "enable-6ghz": {
                        "type": "string",
                        "description": "Flag to set remote leader for 6ghz",
                        "default": "false"
                      }
                    },
                    "required": [
                      "ip-addr",
                      "leader-name",
                      "rf-network-name"
                    ]
                  }
                }
              },
              "example": {
                "wireless_rrm_rpc:rrm-rldr-set": {
                  "ip-addr": "192.168.1.1",
                  "leader-name": "example-string",
                  "rf-network-name": "example-string",
                  "enable-6ghz": "example-string"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                },
