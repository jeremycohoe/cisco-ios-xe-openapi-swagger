    "/operations/wireless_rrm_rpc:rrm-rldr-reset": {
      "post": {
        "summary": "Remote Leader Reset Remote Control Procedure resets the rrm group state from remote-member-state to its pre remote-member-state immediately, as opposed to the heartbeat timer which resets after the timer expires.",
        "description": "Remote Leader Reset Remote Control Procedure resets the rrm group state from remote-member-state to its pre remote-member-state immediately, as opposed to the heartbeat timer which resets after the timer expires.\n\n**Endpoint:** `POST /restconf/operations/wireless_rrm_rpc:rrm-rldr-reset`",
        "operationId": "rrm-rldr-reset",
        "tags": [
          "Cisco-IOS-XE-wireless-rrm-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "wireless_rrm_rpc:rrm-rldr-reset": {
                    "type": "object",
                    "properties": {
                      "reason": {
                        "type": "string",
                        "description": "Remote Leader disconnect reason"
                      },
                      "enable-6ghz": {
                        "type": "string",
                        "description": "Flag to set remote leader for 6ghz",
                        "default": "false"
                      }
                    },
                    "required": [
                      "reason"
                    ]
                  }
                }
              },
              "example": {
                "wireless_rrm_rpc:rrm-rldr-reset": {
                  "reason": "example-string",
                  "enable-6ghz": "example-string"
                }
              }
            }
