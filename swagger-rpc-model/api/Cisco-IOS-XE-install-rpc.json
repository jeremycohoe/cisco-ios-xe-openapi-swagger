  "paths": {
    "/operations/install_rpc:install": {
      "post": {
        "summary": "Software install action command",
        "description": "Software install action command\n\n**Endpoint:** `POST /restconf/operations/install_rpc:install`",
        "operationId": "install",
        "tags": [
          "Cisco-IOS-XE-install-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "install_rpc:install": {
                    "type": "object",
                    "properties": {
                      "vpn": {
                        "type": "string",
                        "description": "The VPN ID is used to indicate device to select the interface\n         for the image download"
                      },
                      "uuid": {
                        "type": "string",
                        "description": "The UUID (universally unique identifier) is a string and is a textual\n         representation of a UUID.\n         UUID is used to uniquely identify the install action invoked via RPC\n         and it is replayed back in the install progress notifications to correlate\n         them back to the original install operation."
                      },
                      "one-shot": {
                        "type": "string",
                        "description": "Software activate and commit in one shot"
                      },
                      "reloadfast": {
                        "type": "string",
                        "description": "This leaf is obsolete. Use new leaf xfsu in the future"
                      },
                      "issu": {
                        "type": "string",
                        "description": "Perform one shot In Service Software Upgrade (ISSU)"
                      },
                      "download-timeout": {
                        "type": "string",
                        "description": "Download timeout is used on device from initiating image download request\n         to complete from remote repository. The image download timeout value is\n         in minutes and in range between 1 and 1440"
                      },
                      "xfsu": {
                        "type": "string",
                        "description": "Perform one shot Extended Fast Software Upgrade (xFSU)"
                      },
                      "scheduled-start": {
                        "type": "string",
                        "description": "Scheduled start time for image install"
                      },
                      "scheduled-end": {
                        "type": "string",
                        "description": "Scheduled end time for image install"
                      },
                      "path": {
                        "type": "string",
                        "minLength": 0,
                        "maxLength": 256,
                        "description": "Path to software image"
                      },
                      "name": {
                        "type": "string",
                        "minLength": 0,
                        "maxLength": 256,
                        "description": "Install profile is used in the wireless image\n             upgrade workflow. Currently it is only used in EWC 9100\n             wireless platform for AP pre-image download using profile\n             on device to upgrade APs in network. Profile name is string"
                      },
                      "controller-mode-options": {
                        "type": "object",
                        "properties": {
                          "vpn": {
                            "type": "string",
                            "description": "The VPN ID is used to indicate device to select the interface\n         for the image download"
                          }
                        },
                        "description": "Controller mode options only"
                      }
                    },
                    "required": [
                      "uuid",
                      "path",
                      "name"
                    ]
                  }
                }
              },
              "example": {
                "install_rpc:install": {
                  "vpn": "GigabitEthernet1/0/1",
                  "uuid": "example-string",
                  "one-shot": "example-string",
                  "reloadfast": "example-string",
                  "issu": "example-string",
                  "download-timeout": "example-string",
                  "xfsu": "example-string",
                  "scheduled-start": "example-string",
                  "scheduled-end": "example-string",
                  "path": "flash:/config.txt",
                  "name": "example-string",
                  "controller-mode-options": {
                    "vpn": "GigabitEthernet1/0/1"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                },
                "example": {}
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
