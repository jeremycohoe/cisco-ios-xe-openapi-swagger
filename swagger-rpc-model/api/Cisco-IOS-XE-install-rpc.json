                      "version": {
                        "type": "string",
                        "description": "Image version to activate"
                      },
                      "path": {
                        "type": "string",
                        "minLength": 0,
                        "maxLength": 256,
                        "description": "Path to the software image"
                      },
                      "name": {
                        "type": "string",
                        "minLength": 0,
                        "maxLength": 256,
                        "description": "Profile name"
                      }
                    },
                    "required": [
                      "uuid",
                      "version",
                      "path",
                      "name"
                    ]
                  }
                }
              },
              "example": {
                "install_rpc:activate": {
                  "uuid": "example-string",
                  "smuid": "example-string",
                  "auto-abort-timer-val": 30,
                  "one-shot": "example-string",
                  "issu": "example-string",
                  "scheduled-start": "example-string",
                  "scheduled-end": "example-string",
                  "version": "example-string",
                  "path": "flash:/config.txt",
