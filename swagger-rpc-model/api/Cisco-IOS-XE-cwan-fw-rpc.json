{
  "openapi": "3.0.0",
  "info": {
    "title": "Cisco-IOS-XE-cwan-fw-rpc - RPC Operations",
    "description": "Copyright (c) 2024-2025 by Cisco Systems, Inc.\n     All rights reserved.\n\n**Module:** `Cisco-IOS-XE-cwan-fw-rpc`\n**Version:** 2025-07-01\n**Operations:** 2\n\n**\ud83d\udcca YANG Tree:** [View Cisco-IOS-XE-cwan-fw-rpc structure](https://jeremycohoe.github.io/cisco-ios-xe-openapi-swagger/yang-trees/Cisco-IOS-XE-cwan-fw-rpc.html)",
    "version": "2025-07-01"
  },
  "servers": [
    {
      "url": "https://{device}/restconf",
      "variables": {
        "device": {
          "default": "devnetsandboxiosxec9k.cisco.com"
        }
      }
    }
  ],
  "paths": {
    "/operations/cwan_fw_rpc:trigger-upgrade": {
      "post": {
        "summary": "Cellular modem firmware action command",
        "description": "Cellular modem firmware action command\n\n**Endpoint:** `POST /restconf/operations/cwan_fw_rpc:trigger-upgrade`",
        "operationId": "trigger-upgrade",
        "tags": [
          "Cisco-IOS-XE-cwan-fw-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "cwan_fw_rpc:trigger-upgrade": {
                    "type": "object",
                    "properties": {
                      "fw-tid": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 255,
                        "pattern": "[\\\\] A-Za-z0-9!\\",
                        "description": "The firmware transaction identifier is used to uniquely identify\n         the modem firmware upgrade action invoked via RPC."
                      },
                      "src-path": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 4095,
                        "pattern": "[\\\\] A-Za-z0-9!\\",
                        "description": "Source path:\n         for download to device: scp://user:password@ipv4 address:/path/to/file\n         or https://ipv4 address/path/to/file\n         or http://ipv4 address/path/to/file\n         or ftp://user:password@ipv4 address:/path/to/file"
                      },
                      "uname": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 63,
                        "description": "Username to be used for remote device"
                      },
                      "passwd": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 63,
                        "description": "Password to be used for remote device"
                      },
                      "vpn": {
                        "type": "string",
                        "minLength": 0,
                        "maxLength": 32,
                        "description": "VPN name instance to reach remote device."
                      },
                      "dnld-timeout": {
                        "type": "string",
                        "description": "Timeout period guarding modem firmware upgrade operation.\n         If operation not completed, download will be aborted after this time."
                      },
                      "schd-start": {
                        "type": "string",
                        "description": "Scheduled modem firmware upgrade start time"
                      },
                      "schd-end": {
                        "type": "string",
                        "description": "Scheduled modem firmware upgrade end time and end time has to be minimum 10 min in future."
                      },
                      "fwtype": {
                        "type": "string",
                        "description": "Firmware type of the embedded AP."
                      },
                      "img-name": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 63,
                        "pattern": "[0-9a-zA-Z\\-._]*",
                        "description": "Specific image to be picked from source path folder.\n         This is used to upgrade or downgrade the firmware to a specific version."
                      }
                    },
                    "required": [
                      "fw-tid",
                      "src-path"
                    ]
                  }
                }
              },
              "example": {
                "cwan_fw_rpc:trigger-upgrade": {
                  "fw-tid": "example-string",
                  "src-path": "flash:/config.txt",
                  "uname": "example-string",
                  "passwd": "example-string",
                  "vpn": "example-string",
                  "dnld-timeout": "example-string",
                  "schd-start": "example-string",
                  "schd-end": "example-string",
                  "fwtype": "example-string",
                  "img-name": "example-string"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "fw-tid": {
                      "type": "string",
                      "description": "The firmware transaction identifier is used to uniquely identify\n         the modem firmware upgrade action invoked via RPC."
                    }
                  }
                },
                "example": {
                  "fw-tid": "example-string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/operations/cwan_fw_rpc:cancel-upgrade": {
      "post": {
        "summary": "Cancel scheduled cellular modem firmware action command",
        "description": "Cancel scheduled cellular modem firmware action command\n\n**Endpoint:** `POST /restconf/operations/cwan_fw_rpc:cancel-upgrade`",
        "operationId": "cancel-upgrade",
        "tags": [
          "Cisco-IOS-XE-cwan-fw-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "cwan_fw_rpc:cancel-upgrade": {
                    "type": "object",
                    "properties": {
                      "fw-tid": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 255,
                        "pattern": "[\\\\] A-Za-z0-9!\\",
                        "description": "The firmware transaction identifier is used to uniquely identify\n         the cancel scheduled modem firmware upgrade action invoked via RPC."
                      }
                    },
                    "required": [
                      "fw-tid"
                    ]
                  }
                }
              },
              "example": {
                "cwan_fw_rpc:cancel-upgrade": {
                  "fw-tid": "example-string"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "fw-tid": {
                      "type": "string",
                      "description": "The firmware transaction identifier is used to uniquely identify\n         the cancel scheduled modem firmware upgrade action invoked via RPC."
                    }
                  }
                },
                "example": {
                  "fw-tid": "example-string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "basicAuth": {
        "type": "http",
        "scheme": "basic"
      }
    }
  },
