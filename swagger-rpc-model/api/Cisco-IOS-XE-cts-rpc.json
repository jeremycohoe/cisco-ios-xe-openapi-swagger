{
  "openapi": "3.0.0",
  "info": {
    "title": "Cisco-IOS-XE-cts-rpc - RPC Operations",
    "description": "NED CTS RPC YANG module for IOS\n     Copyright (c) 2016-2019 by Cisco Systems, Inc.\n     All rights reserved.\n\n**Module:** `Cisco-IOS-XE-cts-rpc`\n**Version:** 2022-11-01\n**Operations:** 1",
    "version": "2022-11-01"
  },
  "servers": [
    {
      "url": "https://{device}/restconf",
      "variables": {
        "device": {
          "default": "router.example.com"
        }
      }
    }
  ],
  "paths": {
    "/operations/cts_rpc:cts": {
      "post": {
        "summary": "Cisco Trusted Security Exec Commands",
        "description": "Cisco Trusted Security Exec Commands\n\n**Endpoint:** `POST /restconf/operations/cts_rpc:cts`",
        "operationId": "cts",
        "tags": [
          "Cisco-IOS-XE-cts-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "cts_rpc:cts": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 24,
                        "description": "Specify the CTS device ID for this switch or router(upto 32 char)"
                      },
                      "password": {
                        "type": "string",
                        "description": "specify this device's CTS password(upto 24 char)"
                      },
                      "credentials": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string",
                            "minLength": 1,
                            "maxLength": 24,
                            "description": "Specify the CTS device ID for this switch or router(upto 32 char)"
                          },
                          "password": {
                            "type": "string",
                            "description": "specify this device's CTS password(upto 24 char)"
                          }
                        },
                        "description": "Insert CTS device ID and password into the keystore"
                      }
                    }
                  }
                }
              },
              "example": {
                "cts_rpc:cts": {
                  "id": "example-string",
                  "password": "Cisco123!",
                  "credentials": {
                    "id": "example-string",
                    "password": "Cisco123!"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                },
                "example": {}
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "basicAuth": {
        "type": "http",
        "scheme": "basic"
      }
    }
  },
  "security": [
    {
      "basicAuth": []
    }
  ]
}