{
  "openapi": "3.0.0",
  "info": {
    "title": "Cisco-IOS-XE-ucse-rpc - RPC Operations",
    "description": "NED UCSE (Unified Computing System E-Series) RPC YANG module for IOS\n     Copyright (c) 2019 by Cisco Systems, Inc.\n     All rights reserved.\n\n**Module:** `Cisco-IOS-XE-ucse-rpc`\n**Version:** 2022-11-01\n**Operations:** 1",
    "version": "2022-11-01"
  },
  "servers": [
    {
      "url": "https://{device}/restconf",
      "variables": {
        "device": {
          "default": "router.example.com"
        }
      }
    }
  ],
  "paths": {
    "/operations/ucse_rpc:ucse": {
      "post": {
        "summary": "UCSE (Unified Computing System E-Series) commands",
        "description": "UCSE (Unified Computing System E-Series) commands\n\n**Endpoint:** `POST /restconf/operations/ucse_rpc:ucse`",
        "operationId": "ucse",
        "tags": [
          "Cisco-IOS-XE-ucse-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "ucse_rpc:ucse": {
                    "type": "object",
                    "properties": {
                      "subslot": {
                        "type": "string",
                        "pattern": "[0-2]/[0-3]",
                        "description": "UCSE (Unified Computing System E-Series) subslot"
                      },
                      "password-reset": {
                        "type": "object",
                        "properties": {
                          "BIOS": {
                            "type": "string",
                            "description": "Reset the BIOS password for the server"
                          }
                        },
                        "description": "Reset the password for the server"
                      },
                      "cmos-reset": {
                        "type": "string",
                        "description": "Reset the CMOS password for the server"
                      },
                      "BIOS": {
                        "type": "string",
                        "description": "Reset the BIOS password for the server"
                      },
                      "reload": {
                        "type": "string",
                        "description": "Reload the server"
                      },
                      "reset": {
                        "type": "string",
                        "description": "Reset the server"
                      },
                      "start": {
                        "type": "string",
                        "description": "Start the server"
                      },
                      "stop": {
                        "type": "string",
                        "description": "Stop the server"
                      },
                      "shutdown": {
                        "type": "string",
                        "description": "Shutdown the UCSE (Unified Computing System E-Series) module"
                      },
                      "clear": {
                        "type": "string",
                        "description": "Clear the statistics"
                      },
                      "imc": {
                        "type": "object",
                        "properties": {
                          "password-reset": {
                            "type": "string",
                            "description": "Reset the password for IMC (Integrated Management Controller)"
                          }
                        },
                        "description": "IMC (Integrated Management Controller) command"
                      },
                      "server": {
                        "type": "object",
                        "properties": {
                          "cmos-reset": {
                            "type": "string",
                            "description": "Reset the CMOS password for the server"
                          },
                          "BIOS": {
                            "type": "string",
                            "description": "Reset the BIOS password for the server"
                          },
                          "reload": {
                            "type": "string",
                            "description": "Reload the server"
                          },
                          "reset": {
                            "type": "string",
                            "description": "Reset the server"
                          },
                          "start": {
                            "type": "string",
                            "description": "Start the server"
                          },
                          "stop": {
                            "type": "string",
                            "description": "Stop the server"
                          },
                          "password-reset": {
                            "type": "object",
                            "properties": {
                              "BIOS": {
                                "type": "string",
                                "description": "Reset the BIOS password for the server"
                              }
                            },
                            "description": "Reset the password for the server"
                          }
                        },
                        "description": "Server commands"
                      },
                      "statistics": {
                        "type": "object",
                        "properties": {
                          "clear": {
                            "type": "string",
                            "description": "Clear the statistics"
                          }
                        },
                        "description": "Statistics command"
                      }
                    },
                    "required": [
                      "subslot"
                    ]
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "basicAuth": {
        "type": "http",
        "scheme": "basic"
      }
    }
  },
  "security": [
    {
      "basicAuth": []
    }
  ]
}