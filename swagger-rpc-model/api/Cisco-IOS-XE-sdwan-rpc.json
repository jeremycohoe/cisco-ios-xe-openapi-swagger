  "paths": {
    "/operations/sdwan_rpc:sdwan-action-audit": {
      "post": {
        "summary": "Audit action RPC",
        "description": "Audit action RPC\n\n**Endpoint:** `POST /restconf/operations/sdwan_rpc:sdwan-action-audit`",
        "operationId": "sdwan-action-audit",
        "tags": [
          "Cisco-IOS-XE-sdwan-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "sdwan_rpc:sdwan-action-audit": {
                    "type": "object",
                    "properties": {
                      "uuid": {
                        "type": "string",
                        "description": "UUID is used to uniquely identify the audit action invoked\n         and it is replayed back in the response notifications to correlate\n         them back to the original operation."
                      },
                      "route-type": {
                        "type": "string",
                        "description": "OMP route types to dump"
                      },
                      "file-format": {
                        "type": "string",
                        "description": "Audit file format to dump"
                      },
                      "file-to-delete": {
                        "type": "string",
                        "description": "Audit file to delete"
                      }
                    }
                  }
                }
              },
              "example": {
                "sdwan_rpc:sdwan-action-audit": {
                  "uuid": "example-string",
                  "route-type": "example-string",
                  "file-format": "flash:/config.txt",
                  "file-to-delete": "flash:/config.txt"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                },
                "example": {}
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
  },
