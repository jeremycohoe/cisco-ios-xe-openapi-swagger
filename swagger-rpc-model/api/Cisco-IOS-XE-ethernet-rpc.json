  "paths": {
    "/operations/ethernet_rpc:ethernet-oam": {
      "post": {
        "summary": "Start/Stop ethernet OAM remote-loopback on interface",
        "description": "Start/Stop ethernet OAM remote-loopback on interface\n\n**Endpoint:** `POST /restconf/operations/ethernet_rpc:ethernet-oam`",
        "operationId": "ethernet-oam",
        "tags": [
          "Cisco-IOS-XE-ethernet-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "ethernet_rpc:ethernet-oam": {
                    "type": "object",
                    "properties": {
                      "action": {
                        "type": "string",
                        "description": "Start or stop action for remote-loopback"
                      },
                      "interface": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 99,
                        "pattern": "[0-9a-zA-Z/\\-_]*",
                        "description": "Interface name to start/stop ethernet OAM remote-loopback"
                      }
                    },
                    "required": [
                      "action",
                      "interface"
                    ]
                  }
                }
              },
              "example": {
                "ethernet_rpc:ethernet-oam": {
                  "action": "example-string",
                  "interface": "GigabitEthernet1/0/1"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                },
                "example": {}
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/operations/ethernet_rpc:eth-lat-loopback": {
      "post": {
        "summary": "Ethernet latching-loopback activate/deactivate",
        "description": "Ethernet latching-loopback activate/deactivate\n\n**Endpoint:** `POST /restconf/operations/ethernet_rpc:eth-lat-loopback`",
        "operationId": "eth-lat-loopback",
        "tags": [
          "Cisco-IOS-XE-ethernet-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "ethernet_rpc:eth-lat-loopback": {
                    "type": "object",
                    "properties": {
                      "action": {
                        "type": "string",
                        "description": "Activate/deactivate action for ethernet latching-loopback"
                      },
                      "domain": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 64,
                        "pattern": "[0-9a-zA-Z\\-_]*",
                        "description": "Ethernet CFM (Connectivity fault management) domain name"
                      },
                      "id": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 8191,
                        "description": "Local MEP(Maintenance End Point) id"
                      },
                      "service": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 64,
                        "pattern": "[0-9a-zA-Z\\-_\\^]*",
                        "description": "Ethernet CFM(Connectivity fault management) maintenance\n         association name"
                      },
                      "destination-mac": {
                        "type": "string",
                        "description": "MAC address of remote device"
                      },
                      "timeout": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 90000,
                        "description": "Timeout value of latching loopback in seconds.\n         0 for permanent latching loopback session",
                        "default": "300"
                      },
                      "svc-name": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 64,
                        "pattern": "[0-9a-zA-Z\\-_\\^]*",
                        "description": "Ethernet CFM(Connectivity fault management) maintenance\n             association name"
                      },
                      "svc-number": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 65535,
                        "description": "Ethernet CFM(Connectivity fault management) maintenance\n             association number"
                      },
                      "svc-none": {
                        "type": "string",
                        "description": "No Ethernet CFM(Connectivity fault management) maintenance\n             association option selected if set to true",
                        "default": "true"
                      }
                    },
                    "required": [
                      "action",
                      "domain",
                      "id",
                      "destination-mac",
                      "svc-name",
                      "svc-number"
                    ]
                  }
                }
              },
              "example": {
                "ethernet_rpc:eth-lat-loopback": {
                  "action": "example-string",
                  "domain": "example-string",
                  "id": 10,
                  "service": "example-string",
                  "destination-mac": "00:11:22:33:44:55",
                  "timeout": 30,
                  "svc-name": "example-string",
                  "svc-number": 10,
                  "svc-none": "example-string"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                },
                "example": {}
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
  },
