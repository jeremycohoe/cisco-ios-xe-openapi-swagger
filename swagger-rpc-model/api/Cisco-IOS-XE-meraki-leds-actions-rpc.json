  "paths": {
    "/operations/meraki_leds_actions_rpc:set-system-led-state": {
      "post": {
        "summary": "Set the system LED state for a device.",
        "description": "Set the system LED state for a device.\n\n**Endpoint:** `POST /restconf/operations/meraki_leds_actions_rpc:set-system-led-state`",
        "operationId": "set-system-led-state",
        "tags": [
          "Cisco-IOS-XE-meraki-leds-actions-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "meraki_leds_actions_rpc:set-system-led-state": {
                    "type": "object",
                    "properties": {
                      "chassis-number": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 16,
                        "description": "Chassis number or switch number"
                      },
                      "led-state": {
                        "type": "string",
                        "description": "New system LED state"
                      }
                    },
                    "required": [
                      "led-state"
                    ]
                  }
                }
              },
              "example": {
                "meraki_leds_actions_rpc:set-system-led-state": {
                  "chassis-number": 10,
                  "led-state": "example-string"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                },
                "example": {}
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/operations/meraki_leds_actions_rpc:blink-leds-cmd": {
      "post": {
        "summary": "Trigger for start/stop blinking device front LEDs",
        "description": "Trigger for start/stop blinking device front LEDs\n\n**Endpoint:** `POST /restconf/operations/meraki_leds_actions_rpc:blink-leds-cmd`",
        "operationId": "blink-leds-cmd",
        "tags": [
          "Cisco-IOS-XE-meraki-leds-actions-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "meraki_leds_actions_rpc:blink-leds-cmd": {
                    "type": "object",
                    "properties": {
                      "chassis-number": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 16,
                        "description": "Chassis number or switch number, by default active switch/chassis will be targeted"
                      },
                      "action": {
                        "type": "string",
                        "description": "Blinking start or stop action"
                      },
                      "duration": {
                        "type": "integer",
                        "minimum": 5,
                        "maximum": 1800,
                        "description": "Duration in seconds, After expiry of this period blinking stops",
                        "default": "300"
                      }
                    },
                    "required": [
                      "action"
                    ]
                  }
                }
              },
              "example": {
                "meraki_leds_actions_rpc:blink-leds-cmd": {
                  "chassis-number": 10,
                  "action": "example-string",
                  "duration": 10
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                },
                "example": {}
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
