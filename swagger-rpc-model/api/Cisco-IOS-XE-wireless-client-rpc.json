        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "wireless_client_rpc:apf-ms-delete-all": {
                    "type": "object",
                    "properties": {
                      "zone-id": {
                        "type": "string",
                        "description": "Binding entry zone-id",
                        "default": "0"
                      },
                      "mac-addr": {
                        "type": "string",
                        "description": "MAC address"
                      },
                      "ip-addr": {
                        "type": "string",
                        "description": "IP address"
                      },
                      "user-name": {
                        "type": "string",
                        "description": "Username"
                      }
                    },
                    "required": [
                      "mac-addr",
                      "ip-addr",
                      "user-name"
                    ]
                  }
                }
              },
              "example": {
                "wireless_client_rpc:apf-ms-delete-all": {
                  "zone-id": "example-string",
                  "mac-addr": "00:11:22:33:44:55",
                  "ip-addr": "192.168.1.1",
                  "user-name": "admin"
                }
              }
