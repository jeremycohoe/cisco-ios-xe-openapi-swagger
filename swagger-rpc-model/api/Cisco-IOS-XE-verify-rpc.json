    "title": "Cisco-IOS-XE-verify-rpc - RPC Operations",
    "description": "This module contains a collection of YANG definitions for\n     Verify RPC.\n     Copyright (c) 2021-2022 by Cisco Systems, Inc.\n     All rights reserved.\n\n**Module:** `Cisco-IOS-XE-verify-rpc`\n**Version:** 2022-11-01\n**Operations:** 1\n\n**\ud83d\udcca YANG Tree:** [View Cisco-IOS-XE-verify-rpc structure](https://jeremycohoe.github.io/cisco-ios-xe-openapi-swagger/yang-trees/Cisco-IOS-XE-verify-rpc.html)",
    "version": "2022-11-01"
  },
  "servers": [
    {
      "url": "https://{device}/restconf",
      "variables": {
        "device": {
          "default": "devnetsandboxiosxec9k.cisco.com"
        }
      }
    }
  ],
  "paths": {
    "/operations/verify_rpc:verify": {
      "post": {
        "summary": "This RPC is to be used for calculating checksum of a given file.\n       Also one can provide previously calculated hash and match against the given file.",
        "description": "This RPC is to be used for calculating checksum of a given file.\n       Also one can provide previously calculated hash and match against the given file.\n\n**Endpoint:** `POST /restconf/operations/verify_rpc:verify`",
        "operationId": "verify",
        "tags": [
          "Cisco-IOS-XE-verify-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "verify_rpc:verify": {
                    "type": "object",
                    "properties": {
                      "file-path": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 500,
                        "description": "the file path"
                      },
                      "hash-algo": {
                        "type": "string",
                        "description": "hashing algorithm to be used"
                      },
                      "hash": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 128,
                        "description": "hash to compare with"
                      }
                    },
                    "required": [
                      "file-path",
                      "hash-algo"
                    ]
                  }
                }
              },
              "example": {
                "verify_rpc:verify": {
                  "file-path": "flash:/config.txt",
                  "hash-algo": "example-string",
                  "hash": "example-string"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "uuid": {
                      "type": "string",
                      "description": "The UUID is unique identifier generated per request. This is used\n         in combination with event model, where event notification provide request specific UUID in it.\n         This is useful in matching a  request with event notification."
                    }
                  }
                },
                "example": {
                  "uuid": "example-string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
  },
