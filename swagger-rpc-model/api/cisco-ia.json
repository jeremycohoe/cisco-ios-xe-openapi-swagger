              "schema": {
                "type": "object",
                "properties": {
                  "cisco_ia:sync-from": {
                    "type": "object",
                    "properties": {
                      "sync-defaults": {
                        "type": "string",
                        "description": "Sends the output of\n           \\"
                      },
                      "ignore-presrv-paths": {
                        "type": "string",
                        "description": "Sync everything under /native.\n           Ignore any preserve paths."
