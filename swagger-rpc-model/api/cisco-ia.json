    "/operations/cisco_ia:rollback": {
      "post": {
        "summary": "Replaces the current running configuration\n       file with a saved Cisco IOS XE configuration file.",
        "description": "Replaces the current running configuration\n       file with a saved Cisco IOS XE configuration file.\n\n**Endpoint:** `POST /restconf/operations/cisco_ia:rollback`",
        "operationId": "rollback",
        "tags": [
          "cisco-ia"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "cisco_ia:rollback": {
                    "type": "object",
                    "properties": {
                      "target-url": {
                        "type": "string",
                        "description": "Cisco IOS XE configuration file that is to\n           replace the current running configuration"
                      },
                      "verbose": {
                        "type": "string",
                        "description": "Display a list of the command lines applied by\n           the Cisco IOS XE software parser during each pass\n           of the configuration replace operation. The total\n           number of passes performed is also displayed.",
                        "default": "false"
                      },
                      "nolock": {
                        "type": "string",
                        "description": "Disables the locking of the running\n           configuration file that prevents other users\n           from changing the running configuration\n           during a configuration replace operation.",
                        "default": "false"
                      },
                      "revert-on-error": {
                        "type": "string",
                        "description": "Reverts to the original configuration upon\n           error."
                      },
                      "revert-timer": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 120,
                        "description": "Reverts to the original configuration if\n           specified time elapses."
                      }
                    },
                    "required": [
                      "target-url"
                    ]
                  }
                }
              },
              "example": {
                "cisco_ia:rollback": {
                  "target-url": "flash:/config.txt",
                  "verbose": "example-string",
                  "nolock": "example-string",
                  "revert-on-error": "example-string",
                  "revert-timer": 10
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "result": {
                      "type": "string",
                      "description": "Output returned by the network element"
                    }
                  }
                },
                "example": {
                  "result": "example-string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
