    "/operations/cisco_ia:revert": {
      "post": {
        "summary": "Cancel the timed rollback and trigger the\n       rollback immediately, or to reset parameters\n       for the timed rollback. If no input parameters\n       are supplied, the timed rollback is cancelled\n       and rollback is triggered immediately",
        "description": "Cancel the timed rollback and trigger the\n       rollback immediately, or to reset parameters\n       for the timed rollback. If no input parameters\n       are supplied, the timed rollback is cancelled\n       and rollback is triggered immediately\n\n**Endpoint:** `POST /restconf/operations/cisco_ia:revert`",
        "operationId": "revert",
        "tags": [
          "cisco-ia"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "cisco_ia:revert": {
                    "type": "object",
                    "properties": {
                      "now": {
                        "type": "string",
                        "description": "To cancel the timed rollback and\n           trigger the rollback immediately"
                      },
                      "timer": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 120,
                        "description": "Reset parameters for the timed rollback"
                      },
                      "idle": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 120,
                        "description": "Maximum allowable time period of no activity\n           before reverting to the saved configuration."
                      }
                    }
                  }
                }
              },
              "example": {
                "cisco_ia:revert": {
                  "now": "example-string",
                  "timer": 10,
                  "idle": 10
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "result": {
                      "type": "string",
