    "/operations/cisco_ia:is-syncing": {
      "post": {
        "summary": "Checks to see if sync from the\n       network element to the running data store\n       is in progress.",
        "description": "Checks to see if sync from the\n       network element to the running data store\n       is in progress.\n\n**Endpoint:** `POST /restconf/operations/cisco_ia:is-syncing`",
        "operationId": "is-syncing",
        "tags": [
          "cisco-ia"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "cisco_ia:is-syncing": {
                    "type": "object",
                    "properties": {}
                  }
                }
              },
              "example": {
                "cisco_ia:is-syncing": {}
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "result": {
                      "type": "string",
                      "description": "Output returned by the network element"
                    },
                    "syncing": {
                      "type": "string",
                      "description": "Network element to the data store sync is in progress or not.\n         Configuration request through programmable interface may get rejected\n         when sync is in progress."
                    },
                    "syncs-pending": {
                      "type": "string",
                      "description": "Network element configurations need to be synced to the data store.\n         Do not configure network element through programmable interface\n         until all the pending configurations are synced to the data store."
                    },
                    "running-datastore-locked": {
                      "type": "string",
                      "description": "Running datastore is locked preventing syncs.\n         Sync will continue once lock is released."
                    },
                    "candidate-datastore-locked": {
                      "type": "string",
                      "description": "Either candidate datastore is locked preventing syncs.\n         Sync will continue once lock is released.\n         This value is returned only in candidate datastore mode."
                    },
                    "candidate-datastore-dirty": {
                      "type": "string",
                      "description": "Synchronization of the running configuration to the running\n         datastore is deferred because the candidate datastore is dirty.\n         Commit or discard the uncommitted configuration.\n         This value is returned only in candidate datastore mode."
                    }
                  }
                },
                "example": {
                  "result": "example-string",
                  "syncing": "GigabitEthernet1/0/1",
                  "syncs-pending": "GigabitEthernet1/0/1",
                  "running-datastore-locked": "example-string",
                  "candidate-datastore-locked": "example-string",
                  "candidate-datastore-dirty": "example-string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
