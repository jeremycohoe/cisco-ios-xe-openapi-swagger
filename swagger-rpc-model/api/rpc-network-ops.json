    "/operations/cisco_bridge_domain:clear-mac-address": {
      "post": {
        "summary": "This RPC allows to clear dynamically learnt mac-address\n       entries from the mac-address table.",
        "description": "**Source Module:** bridge-domain\n\nThis RPC allows to clear dynamically learnt mac-address\n       entries from the mac-address table.\n\n**Endpoint:** `POST /restconf/operations/cisco_bridge_domain:clear-mac-address`",
        "operationId": "clear-mac-address",
        "tags": [
          "cisco-bridge-domain"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "cisco_bridge_domain:clear-mac-address": {
                    "type": "object",
                    "properties": {
                      "bd-id": {
                        "type": "string",
                        "description": "Bridge-domain identifier, clear all mac-address entries\n             dynamically learnt on this bridge-domain"
                      },
                      "bg-id": {
                        "type": "string",
                        "description": "Bridge-group identifier, clear all mac-address entries\n             from all bridge-domains under this bridge-group."
                      },
                      "interface": {
                        "type": "string",
                        "description": "Reference to an interface. Clear mac-addesses learnt by\n           by this interface."
                      },
                      "mac-address": {
                        "type": "string",
                        "description": "Clear a specific mac-address entry from the mac-table."
                      },
                      "bridge-domain": {
                        "type": "object",
                        "properties": {
                          "bd-id": {
                            "type": "string",
                            "description": "Bridge-domain identifier, clear all mac-address entries\n             dynamically learnt on this bridge-domain"
                          },
                          "bg-id": {
                            "type": "string",
                            "description": "Bridge-group identifier, clear all mac-address entries\n             from all bridge-domains under this bridge-group."
                          }
                        },
                        "required": [
                          "bd-id",
                          "bg-id"
                        ],
                        "description": "Clear mac-address entries for given bridge-domain(s)."
                      }
                    },
                    "required": [
                      "bd-id",
                      "bg-id"
                    ]
                  }
                }
              },
              "example": {
                "cisco_bridge_domain:clear-mac-address": {
                  "bd-id": "example-string",
                  "bg-id": "example-string",
                  "interface": "GigabitEthernet1/0/1",
                  "mac-address": "00:11:22:33:44:55",
                  "bridge-domain": {
                    "bd-id": "example-string",
                    "bg-id": "example-string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
