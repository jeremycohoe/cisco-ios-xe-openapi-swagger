    "/operations/livetools_actions_rpc:wol-action": {
      "post": {
        "summary": "This RPC is used to test the WAKE-ON-LAN feature execution and it returns\n       with an 'JOB ID' if the RPC execution was successful",
        "description": "**Source Module:** livetools\n\nThis RPC is used to test the WAKE-ON-LAN feature execution and it returns\n       with an 'JOB ID' if the RPC execution was successful\n\n**Endpoint:** `POST /restconf/operations/livetools_actions_rpc:wol-action`",
        "operationId": "wol-action",
        "tags": [
          "Cisco-IOS-XE-livetools-actions-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "livetools_actions_rpc:wol-action": {
                    "type": "object",
                    "properties": {
                      "mac-addr": {
                        "type": "string",
                        "description": "Destination MAC address"
                      },
                      "vlan": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 4094,
                        "description": "Destination VLAN identifier",
                        "default": "1"
                      },
                      "interval": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 3600,
                        "description": "Interval between retries",
                        "default": "120"
                      },
                      "count": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 255,
                        "description": "Number of packets to be sent",
                        "default": "1"
                      }
                    },
                    "required": [
                      "mac-addr"
                    ]
                  }
                }
              },
              "example": {
                "livetools_actions_rpc:wol-action": {
