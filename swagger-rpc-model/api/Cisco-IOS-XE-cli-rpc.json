{
  "openapi": "3.0.0",
  "info": {
    "title": "Cisco-IOS-XE-cli-rpc - RPC Operations",
    "description": "This module contains a collection of YANG definitions\n     for RPCs related to the Command Line Interface (CLI).\n     Copyright (c) 2022 by Cisco Systems, Inc.\n     All rights reserved.\n\n**Module:** `Cisco-IOS-XE-cli-rpc`\n**Version:** 2024-07-01\n**Operations:** 3",
    "version": "2024-07-01"
  },
  "servers": [
    {
      "url": "https://{device}/restconf",
      "variables": {
        "device": {
          "default": "router.example.com"
        }
      }
    }
  ],
  "paths": {
    "/operations/cli_rpc:config-ios-cli-rpc": {
      "post": {
        "summary": "Configure CLIs directly to the IOS, bypassing NETCONF transaction",
        "description": "Configure CLIs directly to the IOS, bypassing NETCONF transaction\n\n**Endpoint:** `POST /restconf/operations/cli_rpc:config-ios-cli-rpc`",
        "operationId": "config-ios-cli-rpc",
        "tags": [
          "Cisco-IOS-XE-cli-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "cli_rpc:config-ios-cli-rpc": {
                    "type": "object",
                    "properties": {
                      "config-clis": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 60000,
                        "description": "Configuration CLI commands"
                      },
                      "timeout": {
                        "type": "integer",
                        "minimum": 10,
                        "maximum": 1800,
                        "description": "Configurable timeout for the RPC",
                        "default": "300"
                      },
                      "error-tag": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 64,
                        "description": "Tag returned in the response if parser response matches\n           the error-pattern"
                      },
                      "error-pattern": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 256,
                        "description": "error pattern to check against the parser response."
                      }
                    },
                    "required": [
                      "config-clis"
                    ]
                  }
                }
              },
              "example": {
                "cli_rpc:config-ios-cli-rpc": {
                  "config-clis": "example-string",
                  "timeout": 30,
                  "error-tag": "example-string",
                  "error-pattern": "example-string"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "result": {
                      "type": "string",
                      "description": "Output returned by the network element"
                    },
                    "lineno": {
                      "type": "string",
                      "description": "line number of the failed CLI"
                    },
                    "error-code": {
                      "type": "string",
                      "description": "Error code returned by the parser"
                    },
                    "error-string": {
                      "type": "string",
                      "description": "Error string"
                    },
                    "error-tag": {
                      "type": "string",
                      "minLength": 1,
                      "maxLength": 64,
                      "description": "supplied error tag that matched the error-pattern"
                    }
                  }
                },
                "example": {
                  "result": "example-string",
                  "lineno": "example-string",
                  "error-code": "example-string",
                  "error-string": "example-string",
                  "error-tag": "example-string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/operations/cli_rpc:config-ios-cli-trans": {
      "post": {
        "summary": "Configure using modelled config CLI.\n       Wireless,  app-hosting, telemetry features are not\n       supported through this RPC.",
        "description": "Configure using modelled config CLI.\n       Wireless,  app-hosting, telemetry features are not\n       supported through this RPC.\n\n**Endpoint:** `POST /restconf/operations/cli_rpc:config-ios-cli-trans`",
        "operationId": "config-ios-cli-trans",
        "tags": [
          "Cisco-IOS-XE-cli-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "cli_rpc:config-ios-cli-trans": {
                    "type": "object",
                    "properties": {
                      "clis": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 524288,
                        "description": "Configuration CLI commands"
                      },
                      "do-commit": {
                        "type": "string",
                        "description": "Copy the CLI commands from candidate datastore\n           to running datastore in candidate mode. ",
                        "default": "true"
                      },
                      "operation": {
                        "type": "string",
                        "description": "Operations have the following effects:\n           merge: append the supplied contents of this RPC into the\n                  currently configured datastore\n           selective-replace: overwrite the submode config of the\n                              specified cli in the currently\n                              configured datastore with the\n                              contents of this RPC (supported\n                              use only covers submode modification)\n           full-replace: overwrite the complete configuration\n                         stored in the configured datastore with\n                         the contents of this RPC",
                        "default": "merge"
                      }
                    },
                    "required": [
                      "clis"
                    ]
                  }
                }
              },
              "example": {
                "cli_rpc:config-ios-cli-trans": {
                  "clis": "example-string",
                  "do-commit": "example-string",
                  "operation": "example-string"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "result": {
                      "type": "string",
                      "description": "Output returned by the network element"
                    },
                    "error-message": {
                      "type": "string",
                      "description": "Error returned by the network element"
                    }
                  }
                },
                "example": {
                  "result": "example-string",
                  "error-message": "example-string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/operations/cli_rpc:get-modelled-config-clis": {
      "post": {
        "summary": "Retrieve configuration in CLI format from Running or Candidate\n       datastore. Wireless, app-hosting, telemetry features are not\n       supported through this RPC.",
        "description": "Retrieve configuration in CLI format from Running or Candidate\n       datastore. Wireless, app-hosting, telemetry features are not\n       supported through this RPC.\n\n**Endpoint:** `POST /restconf/operations/cli_rpc:get-modelled-config-clis`",
        "operationId": "get-modelled-config-clis",
        "tags": [
          "Cisco-IOS-XE-cli-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "cli_rpc:get-modelled-config-clis": {
                    "type": "object",
                    "properties": {
                      "datastore": {
                        "type": "string",
                        "description": "Datastore to retrieve configuration.",
                        "default": "running"
                      }
                    }
                  }
                }
              },
              "example": {
                "cli_rpc:get-modelled-config-clis": {
                  "datastore": "example-string"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "result": {
                      "type": "string",
                      "description": "Output returned by network element"
                    },
                    "error-message": {
                      "type": "string",
                      "description": "Error returned by the network element"
                    }
                  }
                },
                "example": {
                  "result": "example-string",
                  "error-message": "example-string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
  },
