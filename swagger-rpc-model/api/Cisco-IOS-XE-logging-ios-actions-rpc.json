  "paths": {
    "/operations/logging_ios_actions_rpc:send-log": {
      "post": {
        "summary": "To send a syslog message.",
        "description": "To send a syslog message.\n\n**Endpoint:** `POST /restconf/operations/logging_ios_actions_rpc:send-log`",
        "operationId": "send-log",
        "tags": [
          "Cisco-IOS-XE-logging-ios-actions-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "logging_ios_actions_rpc:send-log": {
                    "type": "object",
                    "properties": {
                      "facility": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 256,
                        "pattern": "[^ ]*",
                        "description": "Facility name"
                      },
                      "mnemonics": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 256,
                        "pattern": "[^ ]*",
                        "description": "Mnemonics"
                      },
                      "message": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 256,
                        "description": "Logging message"
                      },
                      "severity": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 7,
                        "description": "Severity",
                        "default": "7"
                      }
                    },
                    "required": [
                      "message"
                    ]
                  }
                }
              },
              "example": {
                "logging_ios_actions_rpc:send-log": {
                  "facility": "example-string",
                  "mnemonics": "example-string",
                  "message": "example-string",
                  "severity": 7
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
