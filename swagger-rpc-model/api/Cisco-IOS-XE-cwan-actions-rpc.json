  "paths": {
    "/operations/cwan_actions_rpc:cwan-start-dmlog": {
      "post": {
        "summary": "Start cellular modem DM log collection command",
        "description": "Start cellular modem DM log collection command\n\n**Endpoint:** `POST /restconf/operations/cwan_actions_rpc:cwan-start-dmlog`",
        "operationId": "cwan-start-dmlog",
        "tags": [
          "Cisco-IOS-XE-cwan-actions-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "cwan_actions_rpc:cwan-start-dmlog": {
                    "type": "object",
                    "properties": {
                      "dm-tid": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 255,
                        "pattern": "[\\\\] A-Za-z0-9!\\",
                        "description": "The DM log transaction identifier is used to uniquely identify\n         the modem DM log collection action invoked via RPC"
                      },
                      "if-name": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 64,
                        "pattern": "[0-9a-zA-Z\\-_/]*",
                        "description": "Interface name to start modem DM log"
                      },
                      "radio-reset-enable": {
                        "type": "string",
                        "description": "Radio reset option for DM log collection"
                      },
                      "rotation": {
                        "type": "string",
                        "description": "Log rotation option for DM log collection"
                      },
                      "autostop-enable": {
                        "type": "string",
                        "description": "Enable DM log auto stop timer"
                      },
                      "autostop-timer": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 120,
                        "description": "DM log auto stop timer range 1-120 Minutes"
                      }
                    },
                    "required": [
                      "dm-tid",
                      "if-name"
                    ]
                  }
                }
              },
              "example": {
                "cwan_actions_rpc:cwan-start-dmlog": {
                  "dm-tid": "example-string",
                  "if-name": "GigabitEthernet1/0/1",
                  "radio-reset-enable": "example-string",
                  "rotation": "example-string",
                  "autostop-enable": "example-string",
                  "autostop-timer": 10
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "dm-tid": {
                      "type": "string",
                      "description": "The DM log transaction identifier is used to uniquely identify\n         the modem DM log collection action invoked via RPC"
                    },
                    "status": {
                      "type": "string",
                      "description": "Start DM log response status"
                    },
                    "file-path": {
                      "type": "string",
                      "description": "DM log file path generated on the device"
                    }
                  }
                },
                "example": {
                  "dm-tid": "example-string",
                  "status": "example-string",
                  "file-path": "flash:/config.txt"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
