  "paths": {
    "/operations/geo_rpc:geo-database": {
      "post": {
        "summary": "Geo-Location Database action RPC",
        "description": "Geo-Location Database action RPC\n\n**Endpoint:** `POST /restconf/operations/geo_rpc:geo-database`",
        "operationId": "geo-database",
        "tags": [
          "Cisco-IOS-XE-geo-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "geo_rpc:geo-database": {
                    "type": "object",
                    "properties": {
                      "update-file": {
                        "type": "string",
                        "minLength": 0,
                        "maxLength": 255,
                        "pattern": "(flash:|bootflash:)[-A-Za-z_.0-9/]*[-A-Za-z_0-9]",
                        "description": "Specify the Geo-Location Database update file"
                      },
                      "revert-default": {
                        "type": "string",
                        "description": "Revert to an older Geo-Location Database file"
                      }
                    },
                    "required": [
                      "update-file",
                      "revert-default"
                    ]
                  }
                }
              },
              "example": {
                "geo_rpc:geo-database": {
                  "update-file": "flash:/config.txt",
                  "revert-default": "example-string"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                },
                "example": {}
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "basicAuth": {
        "type": "http",
        "scheme": "basic"
      }
    }
  },
