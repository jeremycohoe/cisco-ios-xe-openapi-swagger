{
  "openapi": "3.0.0",
  "info": {
    "title": "Cisco-IOS-XE-switch-rpc - RPC Operations",
    "description": "NED Switch RPC YANG module for IOS\n     Copyright (c) 2016-2024 by Cisco Systems, Inc.\n     All rights reserved.\n\n**Module:** `Cisco-IOS-XE-switch-rpc`\n**Version:** 2024-07-01\n**Operations:** 1",
    "version": "2024-07-01"
  },
  "servers": [
    {
      "url": "https://{device}/restconf",
      "variables": {
        "device": {
          "default": "router.example.com"
        }
      }
    }
  ],
  "paths": {
    "/operations/switch_rpc:switch": {
      "post": {
        "summary": "<1-15>  Switch Priority",
        "description": "<1-15>  Switch Priority\n\n**Endpoint:** `POST /restconf/operations/switch_rpc:switch`",
        "operationId": "switch",
        "tags": [
          "Cisco-IOS-XE-switch-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "switch_rpc:switch": {
                    "type": "object",
                    "properties": {
                      "stack-mode": {
                        "type": "string",
                        "description": "Reset the switch mode to N+1"
                      },
                      "clear": {
                        "type": "object",
                        "properties": {
                          "stack-mode": {
                            "type": "string",
                            "description": "Reset the switch mode to N+1"
                          }
                        },
                        "description": "Reset the switch mode to N+1"
                      },
                      "switch-number-drop-node-name": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 16
                      },
                      "priority": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 15,
                        "description": "<1-15>  Switch Priority"
                      },
                      "renumber": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 16,
                        "description": "<1-16>  New number of the Switch"
                      },
                      "port": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 2,
                        "description": "<1-2>  Stack port number to enable/disable"
                      },
                      "port-number-drop-node-name": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 2,
                        "description": "<1-2>  Stack port number to enable/disable"
                      },
                      "enable": {
                        "type": "string",
                        "description": "Enable stack port"
                      },
                      "disable": {
                        "type": "string",
                        "description": "Disable stack port"
                      },
                      "active": {
                        "type": "string",
                        "description": "Set the new switch to 1+1 active mode"
                      },
                      "standby": {
                        "type": "string",
                        "description": "Set the new switch to 1+1 standby mode"
                      },
                      "statck": {
                        "type": "object",
                        "properties": {
                          "port": {
                            "type": "integer",
                            "minimum": 1,
                            "maximum": 2,
                            "description": "<1-2>  Stack port number to enable/disable"
                          }
                        },
                        "description": "<1-2>  Stack port number to enable/disable"
                      },
                      "stack": {
                        "type": "object",
                        "properties": {
                          "port-number-drop-node-name": {
                            "type": "integer",
                            "minimum": 1,
                            "maximum": 2,
                            "description": "<1-2>  Stack port number to enable/disable"
                          },
                          "enable": {
                            "type": "string",
                            "description": "Enable stack port"
                          },
                          "disable": {
                            "type": "string",
                            "description": "Disable stack port"
                          },
                          "port": {
                            "type": "object",
                            "properties": {
                              "port-number-drop-node-name": {
                                "type": "integer",
                                "minimum": 1,
                                "maximum": 2,
                                "description": "<1-2>  Stack port number to enable/disable"
                              },
                              "enable": {
                                "type": "string",
                                "description": "Enable stack port"
                              },
                              "disable": {
                                "type": "string",
                                "description": "Disable stack port"
                              }
                            },
                            "description": "Stack port enable or disable"
                          }
                        },
                        "description": "Stack port enable or disable"
                      },
                      "role": {
                        "type": "object",
                        "properties": {
                          "active": {
                            "type": "string",
                            "description": "Set the new switch to 1+1 active mode"
                          },
                          "standby": {
                            "type": "string",
                            "description": "Set the new switch to 1+1 standby mode"
                          }
                        },
                        "description": "Set the new switch to 1+1 active mode"
                      }
                    }
                  }
                }
              },
              "example": {
                "switch_rpc:switch": {
                  "stack-mode": "example-string",
                  "clear": {
                    "stack-mode": "example-string"
                  },
                  "switch-number-drop-node-name": 10,
                  "priority": 10,
                  "renumber": 10,
                  "port": 2,
                  "port-number-drop-node-name": 2,
                  "enable": "example-string",
                  "disable": "example-string",
                  "active": "example-string",
                  "standby": "example-string",
                  "statck": {
                    "port": 2
                  },
                  "stack": {
                    "port-number-drop-node-name": 2,
                    "enable": "example-string",
                    "disable": "example-string",
                    "port": {
                      "port-number-drop-node-name": 2,
                      "enable": "example-string",
                      "disable": "example-string"
                    }
                  },
                  "role": {
                    "active": "example-string",
                    "standby": "example-string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
            "content": {
              "application/yang-data+json": {
                "schema": {
                  "type": "object"
                },
                "example": {}
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "basicAuth": {
        "type": "http",
        "scheme": "basic"
      }
    }
  },
  "security": [
    {
      "basicAuth": []
    }
  ]
}