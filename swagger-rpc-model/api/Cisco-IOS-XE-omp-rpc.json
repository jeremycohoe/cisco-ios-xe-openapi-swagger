  "paths": {
    "/operations/omp_rpc:sdwan-clear-omp": {
      "post": {
        "summary": "A SDWAN specific RPC action to clear the routing information base (RIB)\n       of overlay management protocol (OMP)",
        "description": "A SDWAN specific RPC action to clear the routing information base (RIB)\n       of overlay management protocol (OMP)\n\n**Endpoint:** `POST /restconf/operations/omp_rpc:sdwan-clear-omp`",
        "operationId": "sdwan-clear-omp",
        "tags": [
          "Cisco-IOS-XE-omp-rpc"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/yang-data+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "omp_rpc:sdwan-clear-omp": {
                    "type": "object",
                    "properties": {
                      "tenant-id": {
                        "type": "integer",
                        "minimum": 0,
                        "maximum": 65534,
                        "description": "Tenant ID, default 0",
                        "default": "0"
                      },
                      "address": {
                        "type": "string",
                        "description": "IPv4 address of a peer"
                      },
                      "soft": {
                        "type": "string",
                        "description": "Re-evaluate the peer's information with a soft reset.\n         If not specify this value, it will take a hard reset."
                      },
                      "vpn": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 65531,
                        "description": "Layer2 VPN ID"
                      },
                      "clear-all-vpn": {
                        "type": "string",
                        "description": "Clear OMP's layer2 routes with all configured L2VPN"
                      },
                      "vc-id": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 4294967295,
                        "description": "Virtual circuit ID"
                      },
                      "tlocs": {
                        "type": "string",
                        "description": "Clear OMP's transport locators"
                      },
                      "identity": {
                        "type": "string",
                        "description": "Clear OMP's identity"
                      },
                      "all": {
                        "type": "string",
                        "description": "Clear OMP's all RIB"
                      },
                      "peer": {
                        "type": "object",
                        "properties": {
                          "tenant-id": {
                            "type": "integer",
                            "minimum": 0,
                            "maximum": 65534,
                            "description": "Tenant ID"
                          },
                          "address": {
                            "type": "string",
                            "description": "IPv4 address of a peer"
                          },
                          "soft": {
                            "type": "string",
                            "description": "Re-evaluate the peer's information with a soft reset.\n         If not specify this value, it will take a hard reset."
                          }
                        },
                        "required": [
                          "address"
                        ],
                        "description": "Clear OMP's peer"
                      },
                      "routes": {
                        "type": "object",
                        "properties": {
                          "tenant-id": {
                            "type": "integer",
                            "minimum": 0,
                            "maximum": 65534,
                            "description": "Tenant ID, default 0",
                            "default": "0"
                          }
                        },
                        "description": "Clear OMP's routes"
                      },
                      "multicast": {
                        "type": "object",
                        "properties": {
                          "vpn": {
                            "type": "integer",
                            "minimum": 0,
                            "maximum": 65531,
                            "description": "VPN ID"
                          }
                        },
                        "required": [
                          "vpn"
                        ],
                        "description": "Clear OMP's multicast"
                      },
                      "l2-routes": {
                        "type": "object",
                        "properties": {
                          "clear-all-vpn": {
                            "type": "string",
                            "description": "Clear OMP's layer2 routes with all configured L2VPN"
                          },
                          "vpn": {
                            "type": "integer",
                            "minimum": 1,
                            "maximum": 65531,
                            "description": "Layer2 VPN ID"
                          },
                          "vc-id": {
                            "type": "integer",
                            "minimum": 1,
                            "maximum": 4294967295,
                            "description": "Virtual circuit ID"
                          }
                        },
                        "description": "Clear OMP's layer2 routes"
                      }
                    },
                    "required": [
                      "address",
                      "vpn",
                      "tlocs",
                      "identity",
                      "all"
                    ]
                  }
                }
              },
              "example": {
                "omp_rpc:sdwan-clear-omp": {
                  "tenant-id": 10,
                  "address": "example-string",
                  "soft": "example-string",
                  "vpn": 10,
                  "clear-all-vpn": "example-string",
                  "vc-id": 10,
                  "tlocs": "example-string",
                  "identity": "example-string",
                  "all": "example-string",
                  "peer": {
                    "tenant-id": 10,
                    "address": "example-string",
                    "soft": "example-string"
                  },
                  "routes": {
                    "tenant-id": 10
                  },
                  "multicast": {
                    "vpn": 10
                  },
                  "l2-routes": {
                    "clear-all-vpn": "example-string",
                    "vpn": 10,
                    "vc-id": 10
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RPC executed successfully",
