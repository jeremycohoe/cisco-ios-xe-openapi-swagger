  "components": {
    "securitySchemes": {
      "basicAuth": {
        "type": "http",
        "scheme": "basic"
      }
    },
    "schemas": {
      "Cisco-IOS-XE-grpc-tunnel-cfg-grpc-tunnel-cfg-data": {
        "type": "object",
        "properties": {
          "target-type": {
            "type": "string",
            "description": "The gRPC tunnel target being configured"
          },
          "enable": {
            "type": "boolean",
            "description": "Enable or disable this gRPC tunnel destination"
          },
          "port-override": {
            "type": "boolean",
            "description": "Use the provided port-override-value for this target type instead of the default port value."
          },
          "port-override-value": {
            "type": "integer",
            "minimum": 0,
            "maximum": 65535,
            "description": "For use with port-override. Use a custom port number for this target type"
          },
          "server-id": {
            "type": "string",
            "description": "User-provided identifier for this gRPC tunnel destination configuration"
          },
          "src-vrf": {
            "type": "string",
            "description": "The Virtual Routing and Forwarding (VRF) instance to use for this connection. The default VRF will be used if src-vrf is empty"
          },
          "src-addr": {
            "type": "string",
            "description": "The source address to use for outbound gRPC tunnel connections. The address associated with the Mgmt-intf will be used if this is unspecified"
          },
          "destination-port": {
            "type": "integer",
            "minimum": 0,
            "maximum": 65535,
            "description": "The destination port that will be used for the outbound gRPC connection"
          },
          "unspecified": {
            "type": "boolean",
            "description": "The destination address is unspecified"
          },
          "dest-addr": {
            "type": "string",
            "description": "The destination is specified using an IP address"
          },
          "insecure": {
            "type": "boolean",
            "description": "The outgoing connection to the destination is insecure"
          },
          "identity-tsname": {
            "type": "string",
            "description": "The outgoing connection to the destination is secured with TLS"
          }
        }
      },
      "Cisco-IOS-XE-grpc-tunnel-cfg-grpctunnel-target-config": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "target-type": {
              "type": "string",
              "description": "The gRPC tunnel target being configured"
            },
            "enable": {
              "type": "boolean",
              "description": "Enable or disable the target for use with gRPC tunnels"
            },
            "port-override": {
              "type": "boolean",
