  "components": {
    "securitySchemes": {
      "basicAuth": {
        "type": "http",
        "scheme": "basic"
      }
    },
    "schemas": {
      "Cisco-IOS-XE-wireless-mobility-cfg-mobility-cfg-data": {
        "type": "object",
        "properties": {
          "mac-addr": {
            "type": "string",
            "pattern": "^([0-9A-Fa-f]{2}[:-]){5}[0-9A-Fa-f]{2}$",
            "description": "Mobility peer MAC address"
          },
          "ip-address": {
            "type": "string",
            "description": "Peer IP Address"
          },
          "group-name": {
            "type": "string",
            "description": "Group name for remote mobility group"
          },
          "nat-ip": {
            "type": "string",
            "description": "Peer Public IP"
          },
          "data-link-encryption": {
            "type": "boolean",
            "description": "Peer data link encryption status"
          },
          "ssc-hash": {
            "type": "string",
            "description": "Peer SSC HASH"
          },
          "local-group": {
            "type": "string",
            "description": "Local Mobility group name"
          },
          "local-mcast-addr-enabled": {
            "type": "boolean",
            "description": "Enable IPv4 multicast support for local mobility group"
          },
          "local-multicast-address": {
            "type": "string",
            "pattern": "^(?:[0-9]{1,3}\\.){3}[0-9]{1,3}$",
            "description": "Local mobility Multicast IPv4 address"
          },
          "local-ipv6-mcast-addr-enabled": {
            "type": "boolean",
            "description": "Enable IPv6 multicast support for local mobility group"
          },
          "local-ipv6-multicast-address": {
            "type": "string",
            "format": "ipv6",
            "description": "Local mobility Multicast IPv6 address"
          },
          "mobility-keepalive-interval": {
            "type": "integer",
            "minimum": 1,
            "maximum": 30,
            "description": "Mobility Keep Alive interval"
          },
          "mobility-keepalive-count": {
            "type": "integer",
            "minimum": 3,
            "maximum": 20,
            "description": "Mobility Keep Alive count"
          },
          "mobility-dscp": {
            "type": "integer",
            "minimum": 0,
            "maximum": 63,
            "description": "mobility dcsp value"
          },
          "mac-address": {
            "type": "string",
            "pattern": "^([0-9A-Fa-f]{2}[:-]){5}[0-9A-Fa-f]{2}$",
            "description": "Mobility local Mac Address"
          },
          "mm-dtls-high-cipher": {
            "type": "boolean",
            "description": "Enable/Disable DTLS high cipher for mobility"
          },
          "mobility-peers": {
            "type": "object",
            "properties": {
