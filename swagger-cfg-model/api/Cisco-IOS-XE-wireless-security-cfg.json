  "components": {
    "securitySchemes": {
      "basicAuth": {
        "type": "http",
        "scheme": "basic"
      }
    },
    "schemas": {
      "Cisco-IOS-XE-wireless-security-cfg-lsc-cfg-data": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "Setting this to true will make APs to start LSC provisioning. Setting this to false will make APs to fallback to MIC/SSC"
          },
          "key-size": {
            "type": "string",
            "description": "Size of the keys: Can only be 1024 or 2048"
          },
          "join-attempt": {
            "type": "integer",
            "minimum": 0,
            "maximum": 255,
            "description": "Join attempts before AP fallbacks to MIC from LSC."
          },
          "country": {
            "type": "string",
            "description": "Country name that will be sent as a part of Certificate  Signing Request from the AP"
          },
          "state": {
            "type": "string",
            "description": "State name that will be sent as a part of Certificate  Signing Request from the AP"
          },
          "city": {
            "type": "string",
            "description": "City name that will be sent as a part of Certificate  Signing Request from the AP"
          },
          "domain": {
            "type": "string",
            "description": "Domain name that will be sent as a part of Certificate  Signing Request from the AP"
          },
          "org": {
            "type": "string",
            "description": "Organisation name that will be sent as a part of Certificate  Signing Request from the AP"
          },
          "email-addr": {
            "type": "string",
            "description": "Email address that will be sent as a part of Certificate  Signing Request from the AP"
          },
          "sig-256": {
            "type": "boolean",
            "description": "Enable/Disable 256 bit signature"
          },
          "lsc-tp-name": {
            "type": "string",
            "description": "Trustpoint corresponding to the CA server through which certificates will be provided to AP and WLC for LSC provisioning"
          },
          "lsc-renew-syslog": {
            "type": "boolean",
            "description": "Enable/disable periodic syslogs for LSC renewal"
          },
          "lsc-renew": {
            "type": "object",
            "description": "LSC renew configurations"
          },
          "ap-mac": {
            "type": "string",
            "pattern": "^([0-9A-Fa-f]{2}[:-]){5}[0-9A-Fa-f]{2}$",
